<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Validate_GTag_Structure</fullName>
    <active>true</active>
    <description>Regex validation of GTag</description>
    <errorConditionFormula>NOT( REGEX( UPPER(Company_Vehicle_Identifier__c) , &quot;G[0-9A-Z ]+&quot;))</errorConditionFormula>
    <errorDisplayField>Company_Vehicle_Identifier__c</errorDisplayField>
    <errorMessage>Company Vehicle Identifier (G-Tag) must be entered properly.</errorMessage>
</ValidationRule>
