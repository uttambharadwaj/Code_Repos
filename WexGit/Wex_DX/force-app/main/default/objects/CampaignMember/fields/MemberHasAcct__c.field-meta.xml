<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>MemberHasAcct__c</fullName>
    <description>Field used to count campaign members where the Acct # field is not null</description>
    <externalId>false</externalId>
    <formula>IF(
   Contact.Account.F1_Account_Number__c &lt;&gt; null, 1,0
)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>MemberHasAcct</label>
    <precision>18</precision>
    <required>false</required>
    <scale>0</scale>
    <type>Number</type>
    <unique>false</unique>
</CustomField>
