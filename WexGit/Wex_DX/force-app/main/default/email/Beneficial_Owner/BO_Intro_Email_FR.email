    }


    .two-column {
      text-align: center;
      font-size: 0;
    }

    .two-column .column {
      width: 100%;
      display: inline-block;
    }

    .two-column .r1l {
      max-width: 320px;
      vertical-align: top;
    }

    .r1l img {
      width: 100%;
      max-width: 300px;
      height: auto;
    }

    .two-column .r1r {
      max-width: 320px;
      vertical-align: top;
    }

    .r1r img {
      width: 100%;
      max-width: 300px;
      height: auto;
    }

    .two-column .r2l {
      max-width: 187px;
      vertical-align: top;
    }

    .r2l img {
      width: 100%;
      max-width: 300px;
      height: auto;
    }

    .two-column .r2r {
      max-width: 453px;
      vertical-align: top;
    }

    .r2r img {
      width: 100%;
      max-width: 300px;
      height: auto;
    }

    .two-column .contents {
      font-size: 14px;
      text-align: left;
    }

