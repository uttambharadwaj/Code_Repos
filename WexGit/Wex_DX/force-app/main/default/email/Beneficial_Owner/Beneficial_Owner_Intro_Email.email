      vertical-align: top;
    }

    .r1r img {
      width: 100%;
      max-width: 300px;
      height: auto;
    }

    .two-column .r2l {
      max-width: 187px;
      vertical-align: top;
    }

    .r2l img {
      width: 100%;
      max-width: 300px;
      height: auto;
    }

    .two-column .r2r {
      max-width: 453px;
      vertical-align: top;
    }

    .r2r img {
      width: 100%;
      max-width: 300px;
      height: auto;
    }

    .two-column .contents {
      font-size: 14px;
      text-align: left;
    }

    .two-column .text {
      padding-top: 10px;
    }

    .optionlist {
      margin-bottom: 0;
    }

    .optionlist li {
