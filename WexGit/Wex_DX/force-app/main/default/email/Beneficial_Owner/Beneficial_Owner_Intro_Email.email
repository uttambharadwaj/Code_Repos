    }




    @media screen and (max-width: 400px) {
      .two-column .r1l,
      .two-column .r1r,
      .three-column .column,
      .four-column .column {
        max-width: 100% !important;
      }
      .two-column img {
        max-width: 100% !important;
      }
      .three-column img {
        max-width: 50% !important;
      }
      .four-column img {
        max-width: 50% !important;
      }
      .inner,
      .innerl,
      .innerr {
        padding: 10px 15px !important;
      }
      .four-column,
      .three-column {
        padding: 10px 5px !important;
      }
    }

    @media screen and (min-width: 401px) {
      .four-column .column {
        max-width: 50% !important;
      }
