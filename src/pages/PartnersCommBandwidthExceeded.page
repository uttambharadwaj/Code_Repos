<apex:page sidebar="false" showChat="false" showHeader="false" standardStylesheets="false" title="{!$Label.site.bandwidth_limit_exceeded}" controller="PartnersCommBandwidthExceededController">
	<apex:composition template="{!$Site.Template}">
		<apex:define name="icon">
			<apex:image url="{!URLFOR($Resource.PartnersCommResources, 'img/warning.gif')}" />
		</apex:define>
		<apex:define name="error">
			<apex:outputText styleClass="title" value="{!$Label.site.limit_exceeded}" />
			<br />
			<br />
			<apex:outputText value="{!$Label.site.sorry_for_inconvenience}" />
			<apex:outputText escape="false" value=" {!$Label.site.get_in_touch}">
				<apex:param value="mailto:{!IF(ISBLANK(Settings.Support_Email__c) = false, Settings.Support_Email__c, $Site.AdminEmailAddress)}" />
				<apex:param value="{!$Label.site.email_us}" />
			</apex:outputText>
		</apex:define>
	</apex:composition>
</apex:page>