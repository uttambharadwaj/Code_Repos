                                                                <apex:param name="selectedFleetDriver" value="{!fleetDriver.Id}" assignTo="{!selectedFleetDriver}" />
                                                            </apex:commandLink>
                                                        </apex:outputPanel>
                                                    </div>
                                                </div><br/>
                                            </div>
                                        </apex:repeat>
                                    </apex:outputPanel>
                                </div>
                            </div>
                            <div class="twelve columns clearfix">
                                <apex:commandLink styleClass="addButton" style="width: 140px;" oncomplete="pageRerenderFunctions()" action="{!addNewFleetDriver}" rerender="driverDetails" type="image/png" value="Add another driver">
                                </apex:commandLink>
                            </div>
                            <div class="twelve columns clearfix">
                                &nbsp;
                            </div>
                        </apex:outputPanel>
                        <apex:outputPanel id="stepFourButtons" rendered="{!currentStep == 4}">
