                                <div class="six columns">
                                    Authorized Contacts
                                </div>
                                <div class="six columns" style="text-align: right;">
                                    <span class="confirmEdit"><apex:commandLink oncomplete="pageRerenderFunctions()" action="{!doNothing}" rerender="subHeaderImage,fleetEnrollmentForm,stepDisplay,overLimitNotification,staticCardImage,movingCardImage" value="Edit">
