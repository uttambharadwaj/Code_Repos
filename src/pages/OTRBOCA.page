                                                <span class="styled-label-required">*</span>
                                            </apex:outputLabel>
                                            <apex:InputField styleClass="styled-input required email" html-tabindex="9" required="true" id="emailAddress" label="" value="{!onlineApplication.Email__c}" html-autocomplete="off" />
                                        </div>
                                        <div class="twelve columns clearfix form-field" data-row="7">
