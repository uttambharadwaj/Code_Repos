                                            <apex:InputField styleClass="styled-input" html-tabindex="12" id="physicalAddressLine2" html-maxlength="40" value="{!onlineApplication.Business_Address_Line_2__c}" />
                                        </div>
                                        <div class="twelve columns clearfix form-field" data-row="3">
                                            <apex:outputLabel for="city" styleClass="styled-label" value="{!IF(languageCode == 'en_us', 'City', $ObjectType.OnlineApplication__c.fields.City__c.label)}">
                                                <span class="styled-label-required">*</span>
                                            </apex:outputLabel>
                                            <apex:InputField styleClass="styled-input required" html-tabindex="13" id="city" required="true" label="" value="{!onlineApplication.City__c}" html-maxlength="40" />
                                        </div>
                                        <div class="twelve columns clearfix form-field" data-row="4">
                                            <apex:outputLabel for="state" styleClass="styled-label" value="{!IF(languageCode == 'en_us', 'State / Province', $ObjectType.OnlineApplication__c.fields.State__c.label)}">
                                                <span class="styled-label-required">*</span>
                                            </apex:outputLabel>
                                            <apex:InputField styleClass="styled-input required" html-tabindex="14" id="state" required="true" value="{!onlineApplication.State__c}" />
                                        </div>
                                        <div class="twelve columns clearfix form-field" data-row="5">
                                            <apex:outputLabel for="postalCode" styleClass="styled-label" value="{!IF(languageCode == 'en_us', 'Postal Code', $ObjectType.OnlineApplication__c.fields.Zip_Code__c.label)}">
                                                <span class="styled-label-required">*</span>
                                            </apex:outputLabel>
                                            <apex:InputField html-placeholder="{!$Label.BOCA_Placeholder_Postal_Code}" styleClass="styled-input required" required="true" html-tabindex="14" html-class="{!IF(onlineApplication.Country__c == null || onlineApplication.Country__c = 'US', 'postal', 'postalCanada')}" id="postalCode" value="{!onlineApplication.Zip_Code__c}" html-maxlength="10" />
