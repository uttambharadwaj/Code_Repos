                                                <span class="styled-label-required">*</span>
                                            </apex:outputLabel>
                                            <apex:InputField styleClass="styled-input required" html-tabindex="7" id="lastName" required="true" label="" value="{!onlineApplication.Contact_Last_Name__c}" html-maxlength="40" />
                                        </div>
