                                                <apex:InputField rendered="{!onlineApplicationOffer.Funding__c == 'Parent Funded'}" styleClass="styled-input" id="countryWithCA" required="true" value="{!onlineApplication.Country__c}">
                                                    <apex:actionSupport event="onchange" rerender="postalCode, state" oncomplete="pageLoadFunctions()"/>
                                                </apex:InputField>
                                                <apex:InputField rendered="{!onlineApplicationOffer.Funding__c != 'Parent Funded'}" html-disabled="true" styleClass="styled-input" id="country" required="true" value="{!onlineApplication.Country__c}">
                                                    <apex:actionSupport event="onchange" rerender="postalCode, state" oncomplete="pageLoadFunctions()"/>
                                                </apex:InputField>
                                            </apex:actionRegion>
                                        </div>
                                        <div class="twelve columns clearfix form-field" data-row="1">
                                            <apex:outputLabel for="physicalAddress" styleClass="styled-label" value="{!IF(languageCode == 'en_us', 'Business Physical Address', $ObjectType.OnlineApplication__c.fields.Business_Street_Address__c.label)}">
                                                <span class="styled-label-required">*</span>
                                            </apex:outputLabel>
                                            <apex:InputField html-placeholder="{!$Label.BOCA_Placeholder_Physical_Address}" styleClass="styled-input required" required="true" html-tabindex="11" id="physicalAddress" html-maxlength="40" value="{!onlineApplication.Business_Street_Address__c}" />
                                        </div>
                                        <div class="twelve columns clearfix form-field" data-row="2">
