                                        </div>
                                        <div class="twelve columns clearfix form-field" data-row="6">
                                            <apex:actionRegion >
                                                <apex:outputLabel for="fleetSize" styleClass="styled-label" value="{!IF(languageCode == 'en_us', 'Number of Trucks', $ObjectType.OnlineApplication__c.fields.Number_of_Trucks__c.label)}">
                                                    <span class="styled-label-required">*</span>
                                                </apex:outputLabel>
                                                <apex:InputField styleClass="styled-input required" html-tabindex="15" required="true" id="fleetSize" label="" value="{!onlineApplication.Number_of_Trucks__c}" html-maxlength="6">
                                                    <apex:actionSupport event="onchange" action="{!setCreditLineRequested}" rerender="creditLineRequested" />
                                                </apex:InputField>
                                            </apex:actionRegion>
                                        </div>
                                        <div class="twelve columns clearfix form-field" data-row="7">
                                            <div class="six columns clearfix form-field">
                                                <apex:outputLabel for="numberOfDrivers" styleClass="styled-label" value="{!$ObjectType.OnlineApplication__c.fields.Number_of_Drivers__c.label}">
