                                                <apex:outputLabel for="companyName" styleClass="styled-label" value="{!IF(languageCode == 'en_us', 'Company Legal Name', $ObjectType.OnlineApplication__c.fields.Legal_Business_Name__c.label)}">
                                                    <span class="styled-label-required">*</span>
                                                </apex:outputLabel>
                                                <apex:InputField html-placeholder="{!$Label.BOCA_Placeholder_Company_Name}" styleClass="styled-input required" required="true" label="" html-tabindex="2" id="companyName" value="{!onlineApplication.Legal_Business_Name__c}" html-maxlength="30" html-autocomplete="off">
                                                    <apex:actionSupport event="onchange" action="{!doNothing}" rerender="none" />
                                                </apex:InputField>
                                            </apex:actionRegion>
                                        </div>
                                        <div class="twelve columns clearfix form-field" data-row="1">
                                            <apex:outputLabel for="doingBusinessAs" styleClass="styled-label" value="{!IF(languageCode == 'en_us', 'Doing Business As', $ObjectType.OnlineApplication__c.fields.Trade_Name__c.label)}"/>
                                            <apex:InputField styleClass="styled-input" html-tabindex="3" label="" id="doingBusinessAs" value="{!onlineApplication.Trade_Name__c}" html-maxlength="30" />
