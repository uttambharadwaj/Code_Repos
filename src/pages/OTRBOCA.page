                                    <div class="section-header">
                                        {!$Label.BOCA_Applicant_Information}
                                    </div>
                                </div>
                                <div class="twelve columns clearfix">
                                    &nbsp;
                                </div>
                                <div class="twelve columns clearfix">
                                    <div class="six columns clearfix">
                                        <apex:outputPanel rendered="{!onlineApplicationOffer.Funding__c == 'Parent Funded'}">
                                            <div class="twelve columns clearfix form-field" data-row="0">
                                                <apex:outputLabel for="carrierID" styleClass="styled-label" value="{!IF(languageCode == 'en_us', 'Existing Carrier ID', $Label.OTR_Carrier_ID)}"/>
                                                <apex:inputText styleClass="styled-input" label="" tabindex="1" id="carrierID" value="{!carrierID}" maxlength="30" html-autocomplete="off">
