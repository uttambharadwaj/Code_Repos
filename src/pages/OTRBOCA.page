                                                    <apex:actionRegion >
                                                        <apex:InputField styleClass="styled-input" style="margin: 12px 0;" html-tabindex="17" id="slipSeat" label="" value="{!onlineApplication.Team_Drivers_Slip_Seat__c}" html-maxlength="6">
                                                            <apex:actionSupport event="onchange" action="{!setCreditLineRequested}" rerender="creditLineRequested" />
                                                        </apex:InputField>
                                                    </apex:actionRegion>
                                                </center>
