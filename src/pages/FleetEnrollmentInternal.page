		<script>
			var $j = jQuery.noConflict();

			var assetsLoc = '{!URLFOR($Asset.SLDS)}';

			$j.aljsInit({
				assetsLocation: assetsLoc,
				scoped: true
			});

			$j(document).ready(function () {

				overridePageMessages();

				$j('[data-aljs="tabs"]').tabs();

				$j('[data-aljs="popover"]').popover();

				pageRerenderFunctions();

			});

			function pageRerenderFunctions() {
				$j('[data-aljs="popover"]').popover();

				$j('.phone').mask('999-999-9999');
				$j('.ssn').mask('000-00-0000');
				$j('.date').mask('00/00/0000');
				$j('.postal').mask('00000-0000');
				$j('.money').mask('A', {
					translation: {
						'A': {pattern: /[0-9\\.,]+/, recursive: true}
					}
				});

				$j('.datePicker').datepicker();


			}

			function triggerFileBrowse(targetFileElement, targetFileNameElement, visibleFileNameElement) {
				$j('[id$=' + targetFileElement + ']').click();

				$j('[id$=' + targetFileElement + ']').on('change', function() {
					var filename = $j(this).val();
					filename = filename.replace(/.*[\/\\]/, '');
					$j('[id$=' + targetFileNameElement + ']').val(filename);
					$j('[id$=' + visibleFileNameElement + ']').val(filename);
				});
			}

			function overridePageMessages() {
				var textureEffect = '';

				$j('.warningM3').addClass('warningMessage');
				$j('.confirmM3').addClass('confirmMessage');
				$j('.errorM3').addClass('errorMessage');
				$j('.infoM3').addClass('infoMessage');

				$j('.errorM3').removeClass('errorM3');
				$j('.confirmM3').removeClass('confirmM3');
				$j('.infoM3').removeClass('infoM3');
				$j('.warningM3').removeClass('warningM3');
			}

			function toggleSection(triggeringElement, elementId) {
				if ($j(triggeringElement).prop('checked')) {
					$j('[id$=' + elementId + ']').css('display', 'block');
				}
				else {
					$j('[id$=' + elementId + ']').css('display', 'none');

					$j('[id$=' + elementId + ']').find('input[type=text], input[type=file], :checkbox, textarea, select').each(function () {
						if ($j(this).is('select')) {
							$j(this).val('');
						}
						else {
							$j(this).val('');
							$j(this).prop('checked', false);

							if ($j(this).data('controls-section') != null) {
								$j('[id$=' + $j(this).data('controls-section') + ']').css('display', 'none');
								$j('[id$=fleetImplementationForm]').parsley().reset();
							}
						}
					});
				}

				pageRerenderFunctions();
			}

			function toggleCheckboxGroup(triggeringElement, groupName) {
				if ($j(triggeringElement).prop('checked')) {
					$j('[id$=fleetImplementationForm]').find('[data-checkbox-group=' + groupName + ']').each(function () {
						if (!this.isEqualNode(triggeringElement)) {
							console.log('hit');
							$j(this).prop('checked', false);

							if ($j(this).data('controls-section') != null) {
								$j('[id$=' + $j(this).data('controls-section') + ']').css('display', 'none');
							}
						}
					});
				}

				pageRerenderFunctions();
			}

			function markRequired(triggeringElement, fieldName) {
				if($j(triggeringElement).prop('checked')) {
					$j('[id$=' + fieldName + ']').prop('required', true);
				}
				else {
					$j('[id$=' + fieldName + ']').prop('required', false);
				}
			}

			function calculateDateRequirementForTFS(date) {

				var momentCurrent = moment();
				var momentFuture = moment(date).format('MM/DD/YYYY');

				console.log(Math.abs(moment.duration(momentCurrent.diff(momentFuture)).asDays()));

				if(Math.abs(moment.duration(momentCurrent.diff(momentFuture)).asDays()) > 90) {
					$j('[id$=feeWaiversTFSFileName]').prop('required', true);
				}
				else {
					$j('[id$=feeWaiversTFSFileName]').prop('required', false);
				}

			}

			function validateOnSubmit() {

				$j('[id$=fleetImplementationForm]').parsley({excluded: ':hidden', errorsWrapper: '<div class="parsleyErrorMessage"></div>'}).validate();

				if($j('[id$=fleetImplementationForm]').parsley().isValid()) {
					$j('[id$=submitButton]').addClass('disabled-button');
					$j('[id$=spinner]').css('display', 'block').height($j(document).height());
					submitForm();
				}
			}

		</script>

