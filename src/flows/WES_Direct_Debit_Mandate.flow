    <formulas>
        <description>Returns a &apos;Valid&apos; or &apos;Invalid&apos; result based on the MOD 10 check.</description>
        <name>AccountNumberValidity</name>
        <dataType>String</dataType>
        <expression>IF(mod(
IF(
VALUE(MID({!BankAcountNumber},1,1))*2 &gt; 9,
MOD(VALUE(MID({!BankAcountNumber},1,1))*2,10) + 1,
VALUE(MID({!BankAcountNumber},1,1))*2
) +
VALUE(MID({!BankAcountNumber},2,1)) +
IF(
VALUE(MID({!BankAcountNumber},3,1))*2 &gt; 9,
MOD(VALUE(MID({!BankAcountNumber},3,1))*2,10) + 1,
VALUE(MID({!BankAcountNumber},3,1))*2
) +
VALUE(MID({!BankAcountNumber},4,1)) +
IF(
VALUE(MID({!BankAcountNumber},5,1))*2 &gt; 9,
MOD(VALUE(MID({!BankAcountNumber},5,1))*2,10)+1,
VALUE(MID({!BankAcountNumber},5,1))*2
) +
VALUE(MID({!BankAcountNumber},6,1)) +
IF(
VALUE(MID({!BankAcountNumber},7,1))*2 &gt; 9,
MOD(VALUE(MID({!BankAcountNumber},7,1))*2,10)+1,
VALUE(MID({!BankAcountNumber},7,1))*2
) +
VALUE(MID({!BankAcountNumber},8,1)),10)
= 0,&quot;Valid&quot;,&quot;Invalid&quot;)</expression>
    </formulas>
    <formulas>
