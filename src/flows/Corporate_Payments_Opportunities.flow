    <formulas>
        <processMetadataValues>
            <name>originalFormula</name>
            <value>
                <stringValue>AND(
     [Opportunity].RecordType.DeveloperName = &quot;EU_Corporate_Payments&quot;, 
    /* If Opportunity is at or above Stage 4+ and was previously below Stage 4 */
    [Opportunity].Probability &gt;= 0.75,
    PRIORVALUE([Opportunity].Probability) &lt; 0.75
)</stringValue>
            </value>
        </processMetadataValues>
        <name>formula_myRule_1</name>
        <dataType>Boolean</dataType>
        <expression>AND(
     {!myVariable_current.RecordType.DeveloperName} = &quot;EU_Corporate_Payments&quot;, 
    /* If Opportunity is at or above Stage 4+ and was previously below Stage 4 */
    {!myVariable_current.Probability} &gt;= 0.75,
    PRIORVALUE({!myVariable_current.Probability}) &lt; 0.75
)</expression>
    </formulas>
    <formulas>
        <processMetadataValues>
            <name>originalFormula</name>
            <value>
                <stringValue>AND(
  OR([Opportunity].RecordType.DeveloperName = &quot;ASIA_Corporate_Payments&quot;,
    [Opportunity].RecordType.DeveloperName = &quot;AU_Corporate_Payments&quot;,
    [Opportunity].RecordType.DeveloperName = &quot;EU_Corporate_Payments&quot;,
    [Opportunity].RecordType.DeveloperName = &quot;NA_Corporate_Payments&quot;),

ISNEW(),

ISNULL([Opportunity].Billing_Street__c),
ISNULL([Opportunity].Billing_City__c),
ISNULL([Opportunity].Billing_State__c),
ISNULL([Opportunity].Billing_Zip_Postal_Code__c),
ISNULL([Opportunity].Billing_Country__c)
)</stringValue>
            </value>
        </processMetadataValues>
        <name>formula_myRule_11</name>
        <dataType>Boolean</dataType>
        <expression>AND(
