    <description>Implements the one Process per object design pattern - add email alert for processed EU applications</description>
    <formulas>
        <processMetadataValues>
            <name>originalFormula</name>
            <value>
                <stringValue>AND(
  [Case].RecordType.DeveloperName = &apos;V_AU_AM&apos;,
  ISCHANGED([Case].OwnerId),
  /* user Owner Id, not name, in case the queue name is changed */
  /* AU Virtual AM Case Queue */
  PRIORVALUE([Case].OwnerId) = &apos;00G70000002qlVBEAY&apos;
)</stringValue>
            </value>
        </processMetadataValues>
        <name>formula_myRule_1</name>
        <dataType>Boolean</dataType>
        <expression>AND(
  {!myVariable_current.RecordType.DeveloperName} = &apos;V_AU_AM&apos;,
  ISCHANGED({!myVariable_current.OwnerId}),
  /* user Owner Id, not name, in case the queue name is changed */
  /* AU Virtual AM Case Queue */
  PRIORVALUE({!myVariable_current.OwnerId}) = &apos;00G70000002qlVBEAY&apos;
)</expression>
    </formulas>
    <formulas>
        <processMetadataValues>
            <name>originalFormula</name>
            <value>
                <stringValue>AND(
  ISNEW(),
  [Case].RecordType.DeveloperName = &apos;WEX_Salesforce_Support_Request&apos;,
  TEXT([Case].Origin) = &apos;Email - Virtual&apos;
)</stringValue>
            </value>
        </processMetadataValues>
        <name>formula_myRule_3</name>
        <dataType>Boolean</dataType>
        <expression>AND(
  ISNEW(),
  {!myVariable_current.RecordType.DeveloperName} = &apos;WEX_Salesforce_Support_Request&apos;,
  TEXT({!myVariable_current.Origin}) = &apos;Email - Virtual&apos;
)</expression>
    </formulas>
    <formulas>
        <processMetadataValues>
            <name>originalFormula</name>
            <value>
                <stringValue>AND(
  [Case].RecordType.DeveloperName = &apos;WEX_Salesforce_Support_Request&apos;,
  TEXT([Case].Origin) = &apos;Email - Virtual&apos;,
  ISCHANGED([Case].Status),
  TEXT([Case].Status) = &apos;Closed&apos;
)</stringValue>
            </value>
        </processMetadataValues>
        <name>formula_myRule_5</name>
        <dataType>Boolean</dataType>
        <expression>AND(
  {!myVariable_current.RecordType.DeveloperName} = &apos;WEX_Salesforce_Support_Request&apos;,
  TEXT({!myVariable_current.Origin}) = &apos;Email - Virtual&apos;,
  ISCHANGED({!myVariable_current.Status}),
  TEXT({!myVariable_current.Status}) = &apos;Closed&apos;
)</expression>
    </formulas>
    <formulas>
        <processMetadataValues>
            <name>originalFormula</name>
            <value>
                <stringValue>AND(
  [Case].RecordType.DeveloperName = &apos;WE_Application&apos;,
  OR(
    ISCHANGED([Case].KYB_Passed__c),
    ISCHANGED([Case].KYC_Passed__c)
  ),
  TEXT([Case].KYB_Passed__c) = &apos;Yes&apos;,
  TEXT([Case].KYC_Passed__c) = &apos;Yes&apos;
)</stringValue>
            </value>
        </processMetadataValues>
        <name>formula_myRule_7</name>
        <dataType>Boolean</dataType>
        <expression>AND(
  {!myVariable_current.RecordType.DeveloperName} = &apos;WE_Application&apos;,
  OR(
    ISCHANGED({!myVariable_current.KYB_Passed__c}),
    ISCHANGED({!myVariable_current.KYC_Passed__c})
  ),
  TEXT({!myVariable_current.KYB_Passed__c}) = &apos;Yes&apos;,
  TEXT({!myVariable_current.KYC_Passed__c}) = &apos;Yes&apos;
)</expression>
    </formulas>
