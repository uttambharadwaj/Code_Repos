    <formulas>
        <processMetadataValues>
            <name>originalFormula</name>
            <value>
                <stringValue>AND(
  OR( 
     [Opportunity].RecordType.Name = &apos;CP Virtual Credit&apos;, 
     [Opportunity].RecordType.Name = &apos;CP Virtual Prepaid MC&apos;, 
     [Opportunity].RecordType.Name = &apos;CP Virtual Prepaid VE&apos;, 
     [Opportunity].RecordType.Name = &apos;CP Virtual Debit MC&apos;, 
     [Opportunity].RecordType.Name = &apos;CP Virtual All In One&apos; 
  ),
   /* if Opportunity is at or above Stage 4+ */
   [Opportunity].Probability &gt;= 0.75,
  /* if Opportunity was below Stage 4+ */
  PRIORVALUE([Opportunity].Probability) &lt; 75
)</stringValue>
            </value>
        </processMetadataValues>
        <name>formula_myRule_1</name>
        <dataType>Boolean</dataType>
        <expression>AND(
  OR( 
     {!myVariable_current.RecordType.Name} = &apos;CP Virtual Credit&apos;, 
     {!myVariable_current.RecordType.Name} = &apos;CP Virtual Prepaid MC&apos;, 
     {!myVariable_current.RecordType.Name} = &apos;CP Virtual Prepaid VE&apos;, 
     {!myVariable_current.RecordType.Name} = &apos;CP Virtual Debit MC&apos;, 
     {!myVariable_current.RecordType.Name} = &apos;CP Virtual All In One&apos; 
  ),
   /* if Opportunity is at or above Stage 4+ */
   {!myVariable_current.Probability} &gt;= 0.75,
  /* if Opportunity was below Stage 4+ */
  PRIORVALUE({!myVariable_current.Probability}) &lt; 75
)</expression>
    </formulas>
    <formulas>
        <processMetadataValues>
            <name>originalFormula</name>
            <value>
                <stringValue>AND(
  /* MasterCard Opportunities */
  [Opportunity].RecordTypeId = &apos;012700000005Yep&apos;,
  OR(
    AND(
      ISCHANGED([Opportunity].StageName),
      TEXT([Opportunity].StageName) = &apos;4) Negotiating&apos;
    ),
    AND(
      ISCHANGED([Opportunity].Probability),
      [Opportunity].Probability = 1
    )
  )
)</stringValue>
            </value>
        </processMetadataValues>
        <name>formula_myRule_11</name>
        <dataType>Boolean</dataType>
        <expression>AND(
  /* MasterCard Opportunities */
  {!myVariable_current.RecordTypeId} = &apos;012700000005Yep&apos;,
  OR(
    AND(
      ISCHANGED({!myVariable_current.StageName}),
      TEXT({!myVariable_current.StageName}) = &apos;4) Negotiating&apos;
    ),
    AND(
      ISCHANGED({!myVariable_current.Probability}),
      {!myVariable_current.Probability} = 1
    )
  )
)</expression>
    </formulas>
    <formulas>
        <processMetadataValues>
            <name>originalFormula</name>
            <value>
                <stringValue>OR(
  ISCHANGED([Opportunity].CloseDate),
  ISCHANGED([Opportunity].Revenue_Commencement__c),
  ISCHANGED([Opportunity].Net_Full_Service_Revenue__c),
  ISCHANGED([Opportunity].Revenue_In_Year__c),
  ISCHANGED([Opportunity].X1st_Contract_Year_Revenue__c),
  ISCHANGED([Opportunity].CurrencyIsoCode)
  /* can&apos;t include Net FSR in criteria as it&apos;s a formula field */
)</stringValue>
            </value>
        </processMetadataValues>
        <name>formula_myRule_3</name>
        <dataType>Boolean</dataType>
        <expression>OR(
  ISCHANGED({!myVariable_current.CloseDate}),
  ISCHANGED({!myVariable_current.Revenue_Commencement__c}),
  ISCHANGED({!myVariable_current.Net_Full_Service_Revenue__c}),
  ISCHANGED({!myVariable_current.Revenue_In_Year__c}),
  ISCHANGED({!myVariable_current.X1st_Contract_Year_Revenue__c}),
  ISCHANGED({!myVariable_current.CurrencyIsoCode})
  /* can&apos;t include Net FSR in criteria as it&apos;s a formula field */
)</expression>
    </formulas>
    <formulas>
        <processMetadataValues>
            <name>originalFormula</name>
            <value>
                <stringValue>AND(
  OR( 
     [Opportunity].RecordType.Name = &apos;CP Virtual Credit&apos;, 
     [Opportunity].RecordType.Name = &apos;CP Virtual Prepaid MC&apos;, 
     [Opportunity].RecordType.Name = &apos;CP Virtual Prepaid VE&apos;, 
     [Opportunity].RecordType.Name = &apos;CP Virtual Debit MC&apos;, 
     [Opportunity].RecordType.Name = &apos;CP Virtual All In One&apos; 
  ),
   [Opportunity].Probability = 0,
  /* if Opportunity was at Stage 3+ */
  PRIORVALUE([Opportunity].Probability) &gt;= 0.5
)</stringValue>
            </value>
        </processMetadataValues>
        <name>formula_myRule_5</name>
        <dataType>Boolean</dataType>
        <expression>AND(
  OR( 
     {!myVariable_current.RecordType.Name} = &apos;CP Virtual Credit&apos;, 
     {!myVariable_current.RecordType.Name} = &apos;CP Virtual Prepaid MC&apos;, 
     {!myVariable_current.RecordType.Name} = &apos;CP Virtual Prepaid VE&apos;, 
     {!myVariable_current.RecordType.Name} = &apos;CP Virtual Debit MC&apos;, 
     {!myVariable_current.RecordType.Name} = &apos;CP Virtual All In One&apos; 
  ),
   {!myVariable_current.Probability} = 0,
  /* if Opportunity was at Stage 3+ */
  PRIORVALUE({!myVariable_current.Probability}) &gt;= 0.5
)</expression>
    </formulas>
    <formulas>
        <processMetadataValues>
            <name>originalFormula</name>
            <value>
                <stringValue>AND(
  OR(
    /* Virtual Credit, Virtual Prepaid MC, Virtual Debit MC */ 
    [Opportunity].RecordTypeId = &apos;012700000005qie&apos;,
    [Opportunity].RecordTypeId = &apos;012700000005qif&apos;,
    [Opportunity].RecordTypeId = &apos;012700000005wUG&apos;,
    /* Virtual Prepaid VE, Virtual All In One */
    [Opportunity].RecordTypeId = &apos;012700000005qig&apos;,
    [Opportunity].RecordTypeId = &apos;012700000005xWw&apos;,
    /* MasterCard , Virtual Credit MC*/
    [Opportunity].RecordTypeId = &apos;012700000005YepAAE&apos;,
    [Opportunity].RecordTypeId = &apos;012700000005ju4&apos;
  ),
  NOT(TEXT([Opportunity].Type) = &apos;Existing Customer - Administration&apos;),
  OR(
    /* Newly created opportunities, which are won */
    AND(
      ISNEW(),
      [Opportunity].Probability = 1
    ),
    /* Stage moved from open to won */
    AND(
      PRIORVALUE([Opportunity].Probability) &lt; 1,
      [Opportunity].Probability = 1
    )
  )
)</stringValue>
            </value>
        </processMetadataValues>
        <name>formula_myRule_7</name>
        <dataType>Boolean</dataType>
        <expression>AND(
  OR(
    /* Virtual Credit, Virtual Prepaid MC, Virtual Debit MC */ 
    {!myVariable_current.RecordTypeId} = &apos;012700000005qie&apos;,
    {!myVariable_current.RecordTypeId} = &apos;012700000005qif&apos;,
    {!myVariable_current.RecordTypeId} = &apos;012700000005wUG&apos;,
    /* Virtual Prepaid VE, Virtual All In One */
    {!myVariable_current.RecordTypeId} = &apos;012700000005qig&apos;,
    {!myVariable_current.RecordTypeId} = &apos;012700000005xWw&apos;,
    /* MasterCard , Virtual Credit MC*/
    {!myVariable_current.RecordTypeId} = &apos;012700000005YepAAE&apos;,
    {!myVariable_current.RecordTypeId} = &apos;012700000005ju4&apos;
  ),
  NOT(TEXT({!myVariable_current.Type}) = &apos;Existing Customer - Administration&apos;),
  OR(
    /* Newly created opportunities, which are won */
    AND(
      ISNEW(),
      {!myVariable_current.Probability} = 1
    ),
    /* Stage moved from open to won */
    AND(
      PRIORVALUE({!myVariable_current.Probability}) &lt; 1,
      {!myVariable_current.Probability} = 1
    )
  )
)</expression>
    </formulas>
    <formulas>
        <processMetadataValues>
            <name>originalFormula</name>
            <value>
                <stringValue>AND(
  [Opportunity].RecordType.Name = &apos;CP Virtual Credit&apos;,
  ISCHANGED([Opportunity].StageName),
  TEXT([Opportunity].StageName) = &apos;7) Project Completed&apos;
)</stringValue>
            </value>
        </processMetadataValues>
        <name>formula_myRule_9</name>
        <dataType>Boolean</dataType>
        <expression>AND(
  {!myVariable_current.RecordType.Name} = &apos;CP Virtual Credit&apos;,
  ISCHANGED({!myVariable_current.StageName}),
  TEXT({!myVariable_current.StageName}) = &apos;7) Project Completed&apos;
)</expression>
    </formulas>
