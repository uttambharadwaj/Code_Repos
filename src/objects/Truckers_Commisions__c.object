    <fields>
        <fullName>Active__c</fullName>
        <description>Calculates whether the Truckers commission is active</description>
        <externalId>false</externalId>
        <formula>IF( Start_Date__c &lt; Date(Year(Today()), Month(Today()),1) &amp;&amp; End_Date__c &gt;= IF(Month(Today()) = 1, Date(Year(Today()) - 1, 12, 1), Date(Year(Today()), Month(Today()) -1, 1)), &quot;Yes&quot;, &quot;No&quot;)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <inlineHelpText>Is this commission active?</inlineHelpText>
        <label>Active</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
