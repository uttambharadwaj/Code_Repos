    <validationRules>
        <fullName>CALL_Completed_Attempt_or_Conversation</fullName>
        <active>true</active>
        <description>Activity Subtype is required for inside sales when the Activity Type is &apos;Call&apos; and status is &apos;completed&apos; (unless system closed the task (Echosign) and result is &quot;Did not make contact&quot;.</description>
        <errorConditionFormula>AND
(
ISPICKVAL( Type,&quot;Call&quot;), 
ISPICKVAL( Status ,&quot;Completed&quot;), 
ISPICKVAL( Activity_Sub_Type__c ,&quot;&quot;),
NOT(CONTAINS(Subject,&quot;Inbound call from&quot;)),
$Profile.Id &lt;&gt; &quot;00e70000000x2Rv&quot;,
$Profile.Id &lt;&gt; &quot;00e70000000x8vc&quot;, 
$Profile.Id &lt;&gt; &quot;00e70000000tKvI&quot;,
$Profile.Id &lt;&gt; &quot;00e70000000x2N0&quot;,
$Profile.Id &lt;&gt; &quot;00e70000000x8ss&quot;,
$Profile.Id &lt;&gt; &quot;00e70000000x2PQ&quot;,   
$User.Id  &lt;&gt; &quot;00570000001tweu&quot;,
RecordTypeId &lt;&gt; &quot;0120g000000HsOc&quot;,				
NOT($Setup.VProfileId__c.Virtual_User__c = TRUE)
)</errorConditionFormula>
        <errorDisplayField>Activity_Sub_Type__c</errorDisplayField>
        <errorMessage>You must pick an Activity Subtype for a completed Call.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>OTR_Implementation_Calls</fullName>
        <active>true</active>
        <description>User is WEX EFS OTR Sales Profile
Task Subject is Implementation
Must Select a Call Outcome</description>
        <errorConditionFormula>AND( 
$User.ProfileId = &quot;00e70000000xEHg&quot;, 
OR( 
CONTAINS(Subject, &quot;Implement&quot;), 
CONTAINS(Subject, &quot;implement&quot;), 
CONTAINS(Subject, &quot;IMPLEMENT&quot;) 
), 
NOT(ISPICKVAL(Status, &quot;Not Started&quot;)), 
ISPICKVAL(CallOutcome__c, &quot;&quot;) 
)</errorConditionFormula>
        <errorDisplayField>CallOutcome__c</errorDisplayField>
        <errorMessage>Please Select A Call Outcome</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Subject_less_than_100_Characters</fullName>
        <active>false</active>
        <description>The subject field cant have more than 100 characters.  This is to support the BI Database integration</description>
        <errorConditionFormula>AND(
LEN( Subject )&gt;=100,
DATEVALUE(CreatedDate) &lt;=  DATEVALUE(&quot;2013,10,21&quot;))</errorConditionFormula>
        <errorDisplayField>Subject</errorDisplayField>
        <errorMessage>The Subject field cant have more than 100 characters</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>WES_Provide_Disqualified_Reason</fullName>
        <active>true</active>
        <description>Must provide a disqualified reason for WES Activities based on the call outcome.</description>
        <errorConditionFormula>AND( 
RecordType.DeveloperName = &apos;WES_Sales_Tasks&apos;, 
LeadStatusforCallOutcome__c = &apos;Disqualified&apos;, 
ISBLANK(TEXT(ReasonDisqualified__c)) 
)</errorConditionFormula>
        <errorDisplayField>ReasonDisqualified__c</errorDisplayField>
        <errorMessage>Please provide a Reason Disqualified</errorMessage>
    </validationRules>
</CustomObject>
