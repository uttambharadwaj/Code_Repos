    <validationRules>
        <fullName>Best_Western_Accomodation_rule</fullName>
        <active>true</active>
        <description>Ensures that accomodation is selected as a product on all Best Western cards</description>
        <errorConditionFormula>AND(ISPICKVAL( Card_Type__c, &quot;BW: Best Western Trade Card&quot;),
NOT( INCLUDES( Product_Restriction_Groups__c , &quot;150: Accommodation&quot;) ))</errorConditionFormula>
        <errorDisplayField>Product_Restriction_Groups__c</errorDisplayField>
        <errorMessage>Accommodation must be selected for Best Western cards</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Best_Western_Product_Rule</fullName>
        <active>true</active>
        <description>Checks that only Accommodation or Food are selected as products for Best Western cards</description>
        <errorConditionFormula>AND(
 ISPICKVAL( Card_Type__c , &quot;BW: Best Western Trade Card&quot;) 
 ,
 or(
 INCLUDES(Product_Restriction_Groups__c,&quot;10: Leaded&quot;),
 INCLUDES(Product_Restriction_Groups__c,&quot;20: Unleaded	&quot;),
 INCLUDES(Product_Restriction_Groups__c,&quot;30: Premium Unleaded	&quot;),
 INCLUDES(Product_Restriction_Groups__c,&quot;40: Diesel&quot;),
 INCLUDES(Product_Restriction_Groups__c,&quot;50: LPG&quot;),
 INCLUDES(Product_Restriction_Groups__c,&quot;52: Ethanol PULP&quot;),
 INCLUDES(Product_Restriction_Groups__c,&quot;53: Ethanol ULP&quot;),
 INCLUDES(Product_Restriction_Groups__c,&quot;54: Low Aromatic Fuel&quot;),
 INCLUDES(Product_Restriction_Groups__c,&quot;60: Oil&quot;),  
INCLUDES(Product_Restriction_Groups__c,&quot;70: Car Wash&quot;),
INCLUDES(Product_Restriction_Groups__c,&quot;80: Insurance&quot;),
INCLUDES(Product_Restriction_Groups__c,&quot;90: Parking&quot;),
INCLUDES(Product_Restriction_Groups__c,&quot;120: Roadside Assistance&quot;),
INCLUDES(Product_Restriction_Groups__c,&quot;130: Service and Repairs&quot;),
INCLUDES(Product_Restriction_Groups__c,&quot;135: Tyres&quot;),
INCLUDES(Product_Restriction_Groups__c,&quot;140: Tolls&quot;),
INCLUDES(Product_Restriction_Groups__c,&quot;170: Car Rental&quot;),
INCLUDES(Product_Restriction_Groups__c,&quot;200: Shop&quot;),
INCLUDES(Product_Restriction_Groups__c,&quot;210: Taxi&quot;),
INCLUDES(Product_Restriction_Groups__c,&quot;220: Liquor&quot;)
 )
 )</errorConditionFormula>
        <errorDisplayField>Product_Restriction_Groups__c</errorDisplayField>
        <errorMessage>Only Accommodation or Beverage products can be selected for Best Western cards</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Cardholder_Email</fullName>
        <active>true</active>
        <description>Error when Cardholders email&apos;s address is blank and card type control = Trade</description>
        <errorConditionFormula>AND
 (
 ISBLANK( AU_Card_Holder_email_address__c),
 ISPICKVAL(AU_Card_Type__c, &apos;Trade&apos;)
 )</errorConditionFormula>
        <errorMessage>Cardholder&apos;s email address must be completed for Trade cards</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Cardholder_Phone</fullName>
        <active>true</active>
        <description>Error when cardholder&apos;s mobile phone number field is blank and card type (control) = Trade</description>
        <errorConditionFormula>and (
 ISPICKVAL( AU_Card_Type__c , &apos;Trade&apos;),
 isblank  ( AU_Cardholder_Mobile_Phone_Number__c )

)</errorConditionFormula>
        <errorMessage>Cardholder&apos;s mobile phone number must be completed.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Driver_Name_Required</fullName>
        <active>true</active>
        <description>It is an error if Card Type (Control) is either &quot;Driver &amp; Vehicle&quot; , &quot;Driver Only&quot; or &quot;Trade&quot;
AND Driver Name is empty</description>
        <errorConditionFormula>AND(
 ISBLANK(AU_Drivers_Name__c),
 OR(
  ISPICKVAL(AU_Card_Type__c, &apos;Driver Only&apos;),
  ISPICKVAL(AU_Card_Type__c, &apos;Driver &amp; Vehicle&apos;),
  ISPICKVAL(AU_Card_Type__c, &apos;Trade&apos;)
 )
)</errorConditionFormula>
        <errorDisplayField>AU_Drivers_Name__c</errorDisplayField>
        <errorMessage>Driver Name must be entered if Driver chosen in Card Type (Control)</errorMessage>
    </validationRules>
</CustomObject>
