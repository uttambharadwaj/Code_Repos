        <errorConditionFormula>AND(
  OR(
    ISCHANGED(Team_Member_Name__c),
    ISCHANGED(New_Team_Member_Role__c)
  ),
  OR(
    ISBLANK(Team_Member_Name__c),
    ISBLANK(TEXT(New_Team_Member_Role__c))
  ),
  AND(
    /* allow Process Builder to clear fields */
    ISBLANK(PRIORVALUE(Team_Member_Name__c)),
    ISBLANK(PRIORVALUE(New_Team_Member_Role__c))
  )
)</errorConditionFormula>
