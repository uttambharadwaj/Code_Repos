    <fields>
        <fullName>Duration__c</fullName>
        <externalId>false</externalId>
        <formula>IF(FLOOR((End__c - Start__c) / 24) &lt; 10, &quot;0&quot; &amp; 
TEXT(FLOOR((End__c - Start__c) / 24)), 
TEXT(FLOOR((End__c - Start__c) / 24))) &amp; &quot;:&quot; &amp; 
IF(MOD(ROUND((End__c - Start__c) * 1440, 0), 60) &lt; 10, &quot;0&quot; &amp; 
TEXT(MOD(ROUND((End__c - Start__c) * 1440, 0), 60)), 
TEXT(MOD(ROUND((End__c - Start__c) * 1440, 0), 60))) &amp; &quot;:&quot; &amp; 
IF(MOD(ROUND((End__c - Start__c) * 86400, 0), 60) &lt; 10, &quot;0&quot; &amp; 
TEXT(MOD(ROUND((End__c - Start__c) * 86400, 0), 60)), 
TEXT(MOD(ROUND((End__c - Start__c) * 86400, 0), 60)))</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Duration</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Text</type>
