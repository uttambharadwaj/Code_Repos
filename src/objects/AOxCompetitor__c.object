    <fields>
        <fullName>NCED_Entered_Today__c</fullName>
        <description>For reports - is a valid Next Contract End Date populating the field</description>
        <externalId>false</externalId>
        <formula>IF(

AND(
NOT(ISBLANK(Next_Contract_End_Date__c)),
Next_Contract_End_Date__c &gt;= TODAY()
),

1,
null

)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>NCED Entered &amp; &gt;= Today</label>
        <precision>18</precision>
        <required>false</required>
        <scale>0</scale>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Number</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Next_Contract_End_Date__c</fullName>
