    </fields>
    <fields>
        <fullName>Of_Addressable_Spend__c</fullName>
        <externalId>false</externalId>
        <inlineHelpText>The share of the customer&apos;s spend that&apos;s being taken by the competitor</inlineHelpText>
        <label>% Of Addressable Spend</label>
        <precision>3</precision>
        <required>false</required>
        <scale>0</scale>
        <trackHistory>true</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Percent</type>
    </fields>
    <fields>
        <fullName>Value_Of_Spend__c</fullName>
        <externalId>false</externalId>
        <formula>Of_Addressable_Spend__c *  FEP__r.Annual_Addressable_Spend__c</formula>
        <label>Value Of Spend</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Currency</type>
    </fields>
