        <fullName>LeadStatusforCallOutcome__c</fullName>
        <description>Used for WES to update Lead Status with Process Builder.</description>
        <externalId>false</externalId>
        <formula>IF(BEGINS(WhoId, &quot;00Q&quot;),  
CASE(
				 TEXT(CallOutcome__c),
				&quot;Not Interested&quot;, &quot;Disqualified&quot;,
				&quot;Invalid&quot;, &quot;Disqualified&quot;,
    &quot;Barred&quot;, &quot;Disqualified&quot;,
	   &quot;Existing Customer&quot;, &quot;Disqualified&quot;,
				&quot;Unanswered&quot;, &quot;Working&quot;,
				&quot;Callback&quot;, &quot;Working&quot;,
				&quot;Qualified&quot;, &quot;Qualified&quot;,
				&quot;&quot;),
			&quot;&quot;
)</formula>
        <label>Lead Status for Call Outcome</label>
        <required>false</required>
        <type>Text</type>
        <unique>false</unique>
