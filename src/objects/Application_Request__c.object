ISBLANK(Guarantor_SSN__c),
REGEX( Guarantor_SSN__c , &quot;[0-9]{3}-[0-9]{2}-[0-9]{4}&quot;),
Created_By_Echosign__c = TRUE
)
)</errorConditionFormula>
        <errorDisplayField>Guarantor_SSN__c</errorDisplayField>
        <errorMessage>SSN must be in this format: 999-99-9999.</errorMessage>
