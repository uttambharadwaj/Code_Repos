    <validationRules>
        <fullName>Application_Signer_First_And_Last_Name</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
 ISPICKVAL(Forward_Application_to_Credit__c,&quot;Yes&quot;),
  Created_By_Echosign__c = FALSE,
OR(
 ISBLANK(Application_Signer_FirstName_Text__c ),
ISBLANK(App_Signer_Last_Name_Text__c )
)
)</errorConditionFormula>
        <errorDisplayField>App_Signer__c</errorDisplayField>
        <errorMessage>Please be sure to pick the App Singer and it has a First and Last Name</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Application_Signer_Phone_and_Email</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
 ISPICKVAL(Forward_Application_to_Credit__c,&quot;Yes&quot;),
  Created_By_Echosign__c = FALSE,
OR(
 ISBLANK(Application_Signer_Phone__c ),
ISBLANK(App_Signer_Email__c )
)
)</errorConditionFormula>
        <errorDisplayField>App_Signer__c</errorDisplayField>
        <errorMessage>Please ensure the App Signer has a Phone and Email address.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Billing_City</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
 ISPICKVAL(Forward_Application_to_Credit__c,&quot;Yes&quot;),
ISBLANK(Billing_City__c ),
 Created_By_Echosign__c = FALSE
)</errorConditionFormula>
        <errorDisplayField>Billing_City__c</errorDisplayField>
        <errorMessage>You must have a Billing City to forward the Application to Credit.</errorMessage>
    </validationRules>
