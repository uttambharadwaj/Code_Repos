ISBLANK( Tax_Payer_ID_number__c  ),
 Created_By_Echosign__c = FALSE
)</errorConditionFormula>
        <errorMessage>You must have a Tax Id number when forwarding an app to credit. Please add it on the Account Record.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Validate_Beneficial_Owner_Exists</fullName>
        <active>true</active>
        <errorConditionFormula>IF(
  NOT(ISPICKVAL(Opportunity__r.LeadSource, &quot;Conversion&quot;)),
    AND( 
    $User.Alias &lt;&gt; &apos;guest&apos;,
    $User.Alias &lt;&gt; &apos;sinte&apos;,
    $Profile.Name  &lt;&gt; &apos;System Administrator&apos;,
    ISPICKVAL(Forward_Application_to_Credit__c,&quot;Yes&quot;), 
      OR(
        Beneficial_Owner_Information__c == null,
        BO_Status__c == &apos;Pending Lexis/Nexis&apos;
      )
    ),
  false
)</errorConditionFormula>
        <errorMessage>Please ensure you have attached a Beneficial Owner record and run Lexis/Nexis verification.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Validate_Checks_Before_Credit_Decision</fullName>
        <active>false</active>
        <errorConditionFormula>AND( 
OR( 
AND(ISCHANGED(Credit_Decision__c),TEXT(Credit_Decision__c)!= null) 
), 
OR( 
TEXT(Sanctions_Check__c) == &apos;Passed&apos;, 
TEXT(Compliance_Decision__c) == &apos;Approved&apos; 
), 
OR( 
TEXT(Fraud_Recommendation__c) == &apos;Recommend Approve&apos;, 
TEXT(Fraud_Decision__c) == &apos;Approved&apos; 
) 
)</errorConditionFormula>
        <errorMessage>Credit Application cannot be decisioned until Fraud and Compliance checks are completed.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Validate_Country_Code_Lengths</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
OR(
AND(
TEXT(Physical_Country__c) != null,
LEN(TEXT(Physical_Country__c)) != 2
),
AND(
TEXT(Billing_Country__c) != null,
LEN(TEXT(Billing_Country__c)) != 2
)
)
)</errorConditionFormula>
        <errorMessage>Physical Country and Billing Country are limited to two characters. Please correct and try again.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Validate_Decision_Code_And_DeclineLetter</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
ISCHANGED(Credit_Decision__c),
CONTAINS(TEXT(Credit_Decision__c), &apos;Decline&apos;),
OR(
AND(
TEXT(Decline_code_1__c) == null,
TEXT(Decline_Code_2__c) == null,
TEXT(Decline_Code_3__c) == null
),
TEXT(Decline_Letter_Types__c) == null
)
)</errorConditionFormula>
        <errorMessage>Please ensure declined applications have at least one Decline Code selected and a Decline Letter Type selected.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Validate_Decision_Code_For_Decision</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
ISCHANGED(Credit_Decision__c),
TEXT(Credit_Decision__c) != &apos;Re-Review&apos;,
TEXT(Credit_Decision__c) != &apos;Incomplete&apos;,
TEXT(Credit_Bureau_Source__c) == null
)</errorConditionFormula>
        <errorDisplayField>Credit_Bureau_Source__c</errorDisplayField>
        <errorMessage>Please select a Decision Code before setting application to decisioned.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Validate_Lexis_Nexis_Verified</fullName>
        <active>false</active>
        <errorConditionFormula>AND( 
TEXT(Forward_Application_to_Credit__c) == &apos;Yes&apos;, 
ISBLANK(Beneficial_Owner_Information__r.Cloned_From_ID__c), 
OR( 
Beneficial_Owner_Information__c == null, 
BO_Status__c == &apos;Pending Lexis/Nexis&apos; 
) 
)</errorConditionFormula>
        <errorMessage>Please run Lexis/Nexis verification before sending to Credit.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Validate_Not_Tandem_Application</fullName>
        <active>true</active>
        <errorConditionFormula>AND( 
OR( 
AND(ISCHANGED(Credit_Bureau_Source__c),TEXT(Credit_Bureau_Source__c) != null), 
AND(ISCHANGED(Credit_Decision__c),TEXT(Credit_Decision__c)!= null) 
), 
App_Log__c != null
)</errorConditionFormula>
        <errorMessage>Application is for OFFIS adjudication.</errorMessage>
    </validationRules>
</CustomObject>
