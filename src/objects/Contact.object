    <validationRules>
        <fullName>Canadian_State_Code</fullName>
        <active>false</active>
        <errorConditionFormula>AND (
OR(MailingCountry = &quot;CA&quot;, MailingCountry=&quot;CAN&quot;, MailingCountry=&quot;Canada&quot;,
LEN(MailingCountry )=0),
OR(
LEN(MailingState) &lt; 2,
NOT(
CONTAINS(&quot;AB:BC:MB:NB:NL:NT:NS:NU:ON:PC:QC:SK:YT&quot;, MailingState)
)))</errorConditionFormula>
        <errorMessage>A valid two-letter province code is required.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Contact_Call_Outcome_Requires_Call_Stage</fullName>
        <active>true</active>
        <description>Call Outcome must be blank if Call Stage = Legacy or is blank.</description>
        <errorConditionFormula>AND( 

OR( 
ISPICKVAL(Call_Stage__c , &quot;Legacy&quot;), 
ISPICKVAL(Call_Stage__c , &quot;&quot;) 
), 

OR( 
ISPICKVAL(Call_Outcome__c , &quot;No Answer (F/U Task)&quot;), 
ISPICKVAL(Call_Outcome__c , &quot;Left Voicemail (F/U Task &amp; EM)&quot;) 
) 

)</errorConditionFormula>
        <errorDisplayField>Call_Outcome__c</errorDisplayField>
        <errorMessage>Call Stage must be &quot;1st Call, 2nd Call, etc.&quot; before you can enter a Call Outcome.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Email_Required</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
  /*
  custom setting - enables rule to be disabled for user, profile or org
  */ 
  $Setup.WE_VRC__c.All_Contact_Disabled__c = FALSE, 

  /* EU records only */
  RecordType.DeveloperName = &apos;CorporatePay&apos;,
  ISBLANK(Email)
)</errorConditionFormula>
        <errorDisplayField>Email</errorDisplayField>
        <errorMessage>Please enter the Contact&apos;s email address. This enables us to market to the Contact &amp; prevent duplicate records from being created.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Lead_Source_Required</fullName>
        <active>true</active>
        <description>Used instead of a page layout while rule should only apply to UK users.</description>
        <errorConditionFormula>AND(
  /* custom setting - enables rule to be disabled for user, profile or org */
  $Setup.WE_VRC__c.All_Contact_Disabled__c = FALSE,
  RecordType.DeveloperName = &apos;CorporatePay&apos;,
  $RecordType.Name = &apos;MasterCard&apos;, 
  $Setup.VProfileId__c.Virtual_EU_NA_Sales_RM__c = True, 
  OR( 
    ISBLANK(TEXT(LeadSource)), 
    TEXT(LeadSource) = &quot;Partner&quot; 
  ), 
  ISBLANK(Partner_Referrer__c) 
)</errorConditionFormula>
        <errorDisplayField>LeadSource</errorDisplayField>
        <errorMessage>Please enter a Lead Source or the Partner Referrer (from separate field) in the field of the Contact which you are creating this Opportunity from</errorMessage>
