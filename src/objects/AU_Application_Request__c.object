    <validationRules>
        <fullName>Account_must_be_Opportunity_Account</fullName>
        <active>true</active>
        <description>It is an error if the chosen Account differs from the Opportunity&apos;s Account</description>
        <errorConditionFormula>AND( 
RecordType.Name &lt;&gt; &apos;EMAP Application Request&apos;, 
Opportunity__r.Account.Id &lt;&gt; Account__c 
)</errorConditionFormula>
        <errorDisplayField>Account__c</errorDisplayField>
        <errorMessage>Account must equal Opportunity&apos;s Account</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>EMAP_AU_App_Decline_reason_mandate</fullName>
        <active>true</active>
        <description>Decline reason is required, if Application status is Declined or Cancelled</description>
        <errorConditionFormula>AND( 
RecordType.Name == &apos;EMAP Application Request&apos;,
ISPICKVAL( AU_Application_Decline_Reason__c  , &apos;&apos;), 
OR( 
ISPICKVAL( Application_Status__c , &apos;Declined&apos;), 
ISPICKVAL( Application_Status__c , &apos;Cancelled&apos;) 
) 
)</errorConditionFormula>
        <errorDisplayField>AU_Application_Decline_Reason__c</errorDisplayField>
        <errorMessage>Please specify decline reason, if Application status is Declined or cancelled.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>EMAP_Billing_State_mandatory</fullName>
        <active>true</active>
        <description>For countries that have states (i.e. State != Country), then State should be not null when Country is not null</description>
        <errorConditionFormula>AND( 
RecordType.Name == &apos;EMAP Application Request&apos;,
ISBLANK( TEXT(Account_Billing_State__c)), 
OR( 
ISPICKVAL(Account_Billing_Country__c, &quot;Australia&quot;),
ISPICKVAL(Account_Billing_Country__c, &quot;Malaysia&quot;)
) 
)</errorConditionFormula>
        <errorDisplayField>Account_Billing_State__c</errorDisplayField>
        <errorMessage>Billing state cannot be empty. Please enter Billing state.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>EMAP_HK_Card_Pin_Validation</fullName>
        <active>true</active>
        <description>Field is Mandatory for all SG &amp; HK applications</description>
        <errorConditionFormula>AND(
ISNEW(),
RecordType.Name == &apos;EMAP Application Request&apos;, 
ISPICKVAL(Card_Pin__c, &apos;&apos;), 
ISPICKVAL(Client_Name__c, &apos;ExxonMobil Hong Kong&apos;) 
)</errorConditionFormula>
        <errorDisplayField>Card_Pin__c</errorDisplayField>
        <errorMessage>This field is mandatory. Please select appropriate value.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>EMAP_Hold_Expiration_Date_required</fullName>
        <active>true</active>
        <description>Hold expiration date is required if Application status is &apos;Hold&apos;</description>
        <errorConditionFormula>AND ( 
  RecordType.Name == &apos;EMAP Application Request&apos;,
 ISPICKVAL(Application_Status__c , &apos;Hold&apos;),
 ISBLANK( Hold_Expiration_Date__c ) 
)</errorConditionFormula>
        <errorDisplayField>Hold_Expiration_Date__c</errorDisplayField>
        <errorMessage>Please enter hold expiration date, if Application status is Hold.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>EMAP_SG_Card_Pin_Validation</fullName>
        <active>true</active>
        <description>Field is Mandatory for all SG &amp; HK applications</description>
        <errorConditionFormula>AND(
RecordType.Name == &apos;EMAP Application Request&apos;, 
ISPICKVAL(Client_Name__c, &apos;ExxonMobil Singapore&apos;),
OR(
ISPICKVAL(Card_Pin__c, &apos;&apos;),
ISPICKVAL(Card_Pin__c, &apos;N&apos;)
)
)</errorConditionFormula>
        <errorDisplayField>Card_Pin__c</errorDisplayField>
        <errorMessage>This field is mandatory. And it should always be &apos;Y&apos;.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>EMAP_Shipping_State_mandatory</fullName>
        <active>true</active>
        <description>For countries that have states (i.e. State != Country), then State should be not null when Country is not null.</description>
        <errorConditionFormula>AND( 
RecordType.Name == &apos;EMAP Application Request&apos;,
ISBLANK(TEXT( Account_Shipping_State__c )), 
OR(
 ISPICKVAL( Account_Shipping_Country__c , &quot;Australia&quot;),
 ISPICKVAL( Account_Shipping_Country__c , &quot;Malaysia&quot;)
) 
)</errorConditionFormula>
        <errorDisplayField>Account_Shipping_State__c</errorDisplayField>
        <errorMessage>Shipping state cannot be empty. Please enter Shipping state.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Opportunity_Pricing_Must_Be_Approved</fullName>
        <active>true</active>
        <description>It is an error if the parent Opportunity&apos;s Pricing Approval Recieved is false</description>
        <errorConditionFormula>AND( 
RecordType.Name == &apos;EMAP Application Request&apos;, 
NOT(Opportunity__r.Pricing_Approval_Received__c) 
)</errorConditionFormula>
        <errorMessage>Opportunity must have Pricing Approval!</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Opportunity_Will_Supply_Credit_Docs</fullName>
        <active>true</active>
        <description>It is an error if Opportunity &quot;Will Supply Credit Documents has not been entered</description>
        <errorConditionFormula>AND( 
RecordType.Name &lt;&gt; &apos;EMAP Application Request&apos;, 
ISBLANK(TEXT(Opportunity__r.AU_Will_supply_credit_documents__c)) 
)</errorConditionFormula>
        <errorMessage>Opportunity &quot;Will Supply Credit Documents?&quot; must be entered</errorMessage>
    </validationRules>
</CustomObject>
