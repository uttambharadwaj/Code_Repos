    <validationRules>
        <fullName>Handover_Checklist_Completed</fullName>
        <active>false</active>
        <errorConditionFormula>AND(

/* Implementation projects only */
RecordTypeId= &apos;012700000005wcK&apos;,

/* Check Checklist is required */
NOT(TEXT(Handover_Checklist_Status__c) = &quot;Not Required&quot;),

NOT(TEXT(Handover_Checklist_Status__c) = &quot;Checklist Completed&quot;),

ISCHANGED(Stage__c),
TEXT(Stage__c) = &quot;Stage 5 – Handover&quot; 

)</errorConditionFormula>
        <errorDisplayField>Handover_Checklist_Status__c</errorDisplayField>
        <errorMessage>Please complete the Handover Checklist, ticking Handover Checklist Complete or select status &quot;Not Required&quot;</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>MCC_Notes_Required</fullName>
        <active>true</active>
        <errorConditionFormula>AND(

  /* 
  Custom Setting - WE Validation Rules Controller - allows rule to be deactivated when the setting is changed 
  */ 
  $Setup.WE_VRC__c.All_Project_Disabled__c = FALSE,

  /*
  below 2 pieces of criteria enable use of Clone button
  */
  NOT(ISNEW()),
  /* Stage__c must be a required field on the page layout, to ensure that whenever user edit&apos;s Project after cloning it, the field is not blank */
  NOT(ISBLANK(TEXT(Stage__c))),

  RecordType.DeveloperName = &apos;Virtual_NA_Implementation&apos;,
  TEXT(MCC_Controls__c) = &quot;N/A&quot;,
  ISBLANK(MCC_Notes__c),
  AND(
    NOT(TEXT(Stage__c) = &apos;Pending Assigment&apos;),
    NOT(TEXT(Stage__c) = &apos;Pending Client Contact&apos;),
    NOT(TEXT(Stage__c) = &apos;Pending Contract&apos;)
  )
)</errorConditionFormula>
        <errorDisplayField>MCC_Notes__c</errorDisplayField>
        <errorMessage>Please complete MCC Notes when MCC Controls is unchecked</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Non_Standard_Implementation_Details</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
  ISCHANGED(Non_Standard_Implementation_Requested__c),
  NOT(ISBLANK(Non_Standard_Implementation_Requested__c)),
  ISBLANK(Non_Standard_Implementation_Notes__c)
)</errorConditionFormula>
        <errorDisplayField>Non_Standard_Implementation_Notes__c</errorDisplayField>
        <errorMessage>Please confirm the requested elements of the non-standard implementation. This summary will be emailed to the onboarding manager for this project.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>QA_Approval_Before_Go_Live</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
  RecordType.DeveloperName = &apos;Implementation&apos;,
  ISBLANK(QA_Complete__c),
  ISCHANGED(Stage__c),
  CASE(
    TEXT(Stage__c),
    /* Stage API names not labels */
    &apos;Stage 4 – Go-live&apos;, 1,
    &apos;Stage 5 – Handover&apos;, 1,
    0
  ) = 1
)</errorConditionFormula>
        <errorDisplayField>QA_Complete__c</errorDisplayField>
        <errorMessage>The Project&apos;s stage cannot be moved beyond &apos;Stage 4 - Testing&apos; until QA Complete has been confirmed.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Reason_Closed</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
TEXT(Stage__c) = &apos;Closed&apos;,
ISBLANK(TEXT(Reason_Closed__c))
)</errorConditionFormula>
        <errorDisplayField>Reason_Closed__c</errorDisplayField>
        <errorMessage>Please select the reason why the project was closed</errorMessage>
    </validationRules>
</CustomObject>
