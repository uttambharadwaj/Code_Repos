    <validationRules>
        <fullName>Partner_Code_required_for_TriBrand</fullName>
        <active>true</active>
        <description>Partner Code is a required field for Enterprise Tribrand</description>
        <errorConditionFormula>AND(
BEGINS( echosign_dev1__Opportunity__r.Program_Name__c,&quot;Enterprise Tri Brand&quot;),
ISBLANK( echosign_dev1__Opportunity__r.Partner_Code__c ))</errorConditionFormula>
        <errorMessage>Partner Code on Opportunity must be: 4 char Branch Code[SPACE]3 Rep initials. 
Close this window and return to Opportunity to correct.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Partner_Code_required_for_TriBrand_CT</fullName>
        <active>true</active>
        <description>Partner Code is a required field for Enterprise Tribrand Commercial Truck</description>
        <errorConditionFormula>AND(
BEGINS( echosign_dev1__Opportunity__r.Program_Name__c,&quot;Ent Comm Truck&quot;),
ISBLANK( echosign_dev1__Opportunity__r.Partner_Code__c ))</errorConditionFormula>
        <errorMessage>Partner Code on Opportunity must be 4 character Branch Code. 
Close this window and return to Opportunity to correct.</errorMessage>
    </validationRules>
