<?xml version="1.0" encoding="UTF-8"?>
<CustomObject xmlns="http://soap.sforce.com/2006/04/metadata">
    <description>Identifies keywords/terms used to filter out junk and out of office email message from creating Cases.</description>
    <fields>
        <fullName>Email_Field__c</fullName>
        <description>Indicate the section of the email message where the keyword is contained.</description>
        <externalId>false</externalId>
        <fieldManageability>DeveloperControlled</fieldManageability>
        <inlineHelpText>Indicate the section of the email message where the keyword is contained.</inlineHelpText>
        <label>Email Field</label>
        <required>false</required>
        <type>Picklist</type>
        <valueSet>
            <restricted>true</restricted>
            <valueSetDefinition>
                <sorted>true</sorted>
                <value>
                    <fullName>Headers</fullName>
                    <default>true</default>
                    <label>Headers</label>
                </value>
                <value>
                    <fullName>Subject</fullName>
                    <default>false</default>
                    <label>Subject</label>
                </value>
                <value>
                    <fullName>TextBody</fullName>
                    <default>false</default>
                    <label>TextBody</label>
                </value>
            </valueSetDefinition>
        </valueSet>
    </fields>
    <fields>
        <fullName>Status__c</fullName>
        <description>Indicates whether the term is included in the filtering process.</description>
        <externalId>false</externalId>
        <fieldManageability>DeveloperControlled</fieldManageability>
        <inlineHelpText>Indicates whether the term is included in the filtering process.</inlineHelpText>
        <label>Status</label>
        <required>false</required>
        <type>Picklist</type>
        <valueSet>
            <restricted>true</restricted>
            <valueSetDefinition>
                <sorted>true</sorted>
                <value>
                    <fullName>Active</fullName>
                    <default>true</default>
                    <label>Active</label>
                </value>
                <value>
                    <fullName>Inactive</fullName>
                    <default>false</default>
                    <label>Inactive</label>
                </value>
            </valueSetDefinition>
        </valueSet>
    </fields>
    <fields>
        <fullName>Term__c</fullName>
        <description>Term the filter will look for</description>
        <externalId>false</externalId>
        <fieldManageability>DeveloperControlled</fieldManageability>
        <inlineHelpText>Term the filter will look for</inlineHelpText>
        <label>Term</label>
        <length>255</length>
        <required>false</required>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <label>Email Discarded Terms</label>
    <listViews>
        <fullName>Active_Mappings</fullName>
        <columns>MasterLabel</columns>
        <columns>DeveloperName</columns>
        <columns>NamespacePrefix</columns>
        <columns>Email_Field__c</columns>
        <columns>Status__c</columns>
        <columns>Term__c</columns>
        <filterScope>Everything</filterScope>
        <filters>
            <field>Status__c</field>
            <operation>equals</operation>
            <value>Active</value>
        </filters>
        <label>Active Mappings</label>
        <language>en_US</language>
    </listViews>
    <listViews>
        <fullName>All_terms</fullName>
        <columns>MasterLabel</columns>
        <columns>DeveloperName</columns>
        <columns>NamespacePrefix</columns>
        <columns>Email_Field__c</columns>
        <columns>Status__c</columns>
        <columns>Term__c</columns>
        <filterScope>Everything</filterScope>
        <label>All terms</label>
        <language>en_US</language>
    </listViews>
    <pluralLabel>Email Discarded Terms</pluralLabel>
    <visibility>Public</visibility>
</CustomObject>
