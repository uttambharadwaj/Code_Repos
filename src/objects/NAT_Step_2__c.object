<?xml version="1.0" encoding="UTF-8"?>
<CustomObject xmlns="http://soap.sforce.com/2006/04/metadata">
    <enableEnhancedLookup>true</enableEnhancedLookup>
    <validationRules>
        <fullName>Equifax_Credit_Score_Range</fullName>
        <active>true</active>
        <description>Equifax Credit Range = Min of 370 and Max of 820</description>
        <errorConditionFormula>OR(
 Equifax_Score__c   &gt;=  320,
Equifax_Score__c  &lt;=  820
)</errorConditionFormula>
        <errorMessage>Equifax Credit Scores must be in a range of 370-820</errorMessage>
    </validationRules>
</CustomObject>
