    <validationRules>
        <fullName>BO_Entity_Virtual_Required</fullName>
        <active>true</active>
        <description>If this is a virtual record, we need to require either a SFDC account/opportunity, or a Siebel opportunity #, or contract #.</description>
        <errorConditionFormula>AND( 
$RecordType.Name = &quot;Virtual&quot;, 
ISBLANK(Account__c), 
ISBLANK(Opportunity__c), 
ISBLANK(Contract_Number__c), 
ISBLANK(Opportunity_Number__c),
ISBLANK(Opportunity_Number__c)
)</errorConditionFormula>
        <errorMessage>Please link this record to an existing Account or Opportunity in Salesforce, or enter an Account #, Contract #, or Siebel Opportunity #.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Verify_Certified_By_Is_Entered</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
AND(
TEXT(Verification_Status__c) != &apos;Pending Lexis/Nexis&apos;,
TEXT(Verification_Status__c) != &apos;Exempted&apos;),
OR(
ISBLANK(Certified_By__c),
ISBLANK(Certified_Date__c)
)
)</errorConditionFormula>
        <errorMessage>Please provided a certified name and date!</errorMessage>
    </validationRules>
