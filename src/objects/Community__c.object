<?xml version="1.0" encoding="UTF-8"?>
<CustomObject xmlns="http://soap.sforce.com/2006/04/metadata">
    <customSettingsType>List</customSettingsType>
    <description>The Name field should be the name of the Site API name.</description>
    <enableFeeds>false</enableFeeds>
    <fields>
        <fullName>Community_Hostname__c</fullName>
        <description>If you want to use SSL (and you likely do want to use SSL), you will need to include the https:// since the SF URL control will use http:// if it is not specified.</description>
        <encryptionScheme>None</encryptionScheme>
        <externalId>false</externalId>
        <inlineHelpText>If you want to use SSL (and you likely do want to use SSL), you will need to include the https:// since the SF URL control will use http:// if it is not specified.</inlineHelpText>
        <label>Community Hostname</label>
        <required>true</required>
        <trackTrending>false</trackTrending>
        <type>Url</type>
    </fields>
    <fields>
        <fullName>Component_Width__c</fullName>
        <defaultValue>758</defaultValue>
        <description>This is used to dynamically configure the width that is used in numerous portal pages provided by SF.</description>
        <externalId>false</externalId>
        <label>Component Width</label>
        <precision>18</precision>
        <required>false</required>
        <scale>0</scale>
        <trackTrending>false</trackTrending>
        <type>Number</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Submit_Form_as_POST__c</fullName>
        <defaultValue>true</defaultValue>
        <description>Flag to toggle between submitting as a POST vs. GET. Submitting as a POST is most secure since a GET will pass the username and password values along the query string.</description>
        <externalId>false</externalId>
        <label>Submit Form as POST</label>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>Support_Email__c</fullName>
        <description>This field is used to override the Site&apos;s support email rather than to use the Site Contact&apos;s email.</description>
        <encryptionScheme>None</encryptionScheme>
        <externalId>false</externalId>
        <inlineHelpText>This field is used to override the Site&apos;s support email rather than to use the Site Contact&apos;s email.</inlineHelpText>
        <label>Support Email</label>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Email</type>
        <unique>false</unique>
    </fields>
    <label>Community</label>
    <visibility>Public</visibility>
</CustomObject>
