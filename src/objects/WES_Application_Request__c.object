    <fields>
        <fullName>BankAccountNumValidity__c</fullName>
        <externalId>false</externalId>
        <formula>IF(ISBLANK(bankAccountNumber__c), NULL,
IF(NOT(ISBLANK(bankAccountNumber__c)) &amp;&amp; mod(
IF(
VALUE(MID(bankAccountNumber__c,1,1))*2 &gt; 9, 
MOD(VALUE(MID(bankAccountNumber__c,1,1))*2,10) + 1,
VALUE(MID(bankAccountNumber__c,1,1))*2
) +
VALUE(MID(bankAccountNumber__c,2,1)) +
IF(
VALUE(MID(bankAccountNumber__c,3,1))*2 &gt; 9,
MOD(VALUE(MID(bankAccountNumber__c,3,1))*2,10) + 1,
VALUE(MID(bankAccountNumber__c,3,1))*2
) +
VALUE(MID(bankAccountNumber__c,4,1)) +
IF(
VALUE(MID(bankAccountNumber__c,5,1))*2 &gt; 9,
MOD(VALUE(MID(bankAccountNumber__c,5,1))*2,10)+1,
VALUE(MID(bankAccountNumber__c,5,1))*2
) +
VALUE(MID(bankAccountNumber__c,6,1)) +
IF(
VALUE(MID(bankAccountNumber__c,7,1))*2 &gt; 9,
MOD(VALUE(MID(bankAccountNumber__c,7,1))*2,10)+1,
VALUE(MID(bankAccountNumber__c,7,1))*2
) +
VALUE(MID(bankAccountNumber__c,8,1)),10)
= 0,&quot;Valid&quot;,&quot;Invalid&quot;))</formula>
        <label>Bank Account # Validity</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
