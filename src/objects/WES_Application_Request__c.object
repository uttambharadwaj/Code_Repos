    <validationRules>
        <fullName>App_must_pass_security_check_to_progress</fullName>
        <active>true</active>
        <description>A Sales agent must submit an approval request to pass the security check to proceed with Credit passing the application.</description>
        <errorConditionFormula>AND(
TEXT(Application_Status__c) &lt;&gt; &quot;Draft&quot;,				
Passed_Security_Check__c = FALSE,
BEGINS($User.Role_Name__c, &quot;WES&quot;),
CONTAINS($User.Role_Name__c, &quot;Sales&quot;) ||
CONTAINS($User.Role_Name__c, &quot;sales&quot;)
)</errorConditionFormula>
        <errorMessage>You must submit an approval request for a Security Check before application can be sent to Credit.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Check_for_Velocity_Profile</fullName>
        <active>true</active>
        <description>The Velocity Profile or Velocity profile override must be set in order to pass the security check.</description>
        <errorConditionFormula>AND(
Passed_Security_Check__c = TRUE,
ISBLANK(VelocityProfileName__c)
				)</errorConditionFormula>
        <errorDisplayField>VelocityProfileOverride__c</errorDisplayField>
        <errorMessage>The Velocity Profile or Velocity Profile override must be set in order to pass the security check.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Credit_Auth_Req_when_Accepted_Decision</fullName>
        <active>true</active>
        <errorConditionFormula>TEXT(Credit_Decision__c) = &quot;Accepted&quot; &amp;&amp;
	ISBLANK(Credit_Authorised_Limit__c)</errorConditionFormula>
        <errorDisplayField>Credit_Authorised_Limit__c</errorDisplayField>
        <errorMessage>You must provide a Credit Authorised Limit Amount when the Credit Decision is &quot;Accepted&quot;</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Credit_Authorised_Limit_Criteria</fullName>
        <active>true</active>
        <description>The Credit Decision must be Accepted or Pending approval in order to enter an Authorised credit limit.</description>
        <errorConditionFormula>AND(
Credit_Authorised_Limit__c &gt; 0,				
NOT(TEXT(Credit_Decision__c) = &quot;Accepted&quot;),
NOT(TEXT(Credit_Decision__c) = &quot;Pending Approval&quot;),				
From_Radius__c = FALSE 				
)</errorConditionFormula>
        <errorMessage>The Credit Decision must be &quot;Accepted&quot; or &quot;Pending Approval&quot; in order to enter an Authorised credit limit.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Criteria_to_Pass_Security_Check</fullName>
        <active>true</active>
        <description>All sections must be complete before Passed Security Check is marked True.</description>
        <errorConditionFormula>AND(
				ISNEW() || ISCHANGED(Passed_Security_Check__c),
				Passed_Security_Check__c = TRUE,
OR(
				 ApplicationDetailsComplete__c = FALSE,
				 ContactInfoComplete__c = FALSE,
				 CompanyInfoComplete__c = FALSE,
				 FuelCardInfoComplete__c = FALSE
				))</errorConditionFormula>
        <errorMessage>All sections must be complete before Passed Security Check is marked True.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Enter_Credit_decision_comments</fullName>
        <active>true</active>
        <description>User must enter Credit Decision comments when making a credit decision</description>
        <errorConditionFormula>AND(
NOT(ISBLANK(TEXT(Credit_Decision__c))),
ISBLANK(Credit_Decision_Comments__c)
	)</errorConditionFormula>
        <errorDisplayField>Credit_Decision_Comments__c</errorDisplayField>
        <errorMessage>You must enter a Credit Decision Comment when making a credit decision.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Limit_1_Default_per_Card_Type</fullName>
        <active>true</active>
        <description>Each Card Type can have at most 1 Default Card</description>
        <errorConditionFormula>OR(
 NumofEDCDefaults__c &gt; 1,
	NumofEssoDefaults__c &gt; 1,
 NumofUKFuelDefaults__c &gt; 1)</errorConditionFormula>
        <errorMessage>You can  have only 1 Default Fuel card per Card Type.
Please edit an existing Default Fuel card.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Need_Decline_Reason</fullName>
        <active>true</active>
        <description>An application decline reason is required when an application is Rejected or Unable to process (non-credit reason).</description>
        <errorConditionFormula>ISBLANK(TEXT(Application_Decline_Reason__c)) &amp;&amp;
(TEXT(Credit_Decision__c ) = &quot;Rejected&quot; ||
TEXT(Credit_Decision__c ) = &quot;Unable to Process&quot;)</errorConditionFormula>
        <errorDisplayField>Application_Decline_Reason__c</errorDisplayField>
        <errorMessage>You must provide a decline reason when the Application is &quot;Reject&quot; or &quot;Unable to Process&quot;.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Prevent_Accepting_App_if_above_Max_Cards</fullName>
        <active>true</active>
        <description>Prevent users from setting Application status to Accepted if the Max Num of Cards can approve is above their user&apos;s limit.</description>
        <errorConditionFormula>AND(
TEXT(Application_Status__c) = &quot;Accepted&quot;,
BEGINS($UserRole.Name , &quot;WES Credit&quot;),
CreditLevelRequestApproved__c = FALSE,
Total_Fuel_Cards_Needed__c &gt;  $User.WES_MaxCardsCanApprove__c 
)</errorConditionFormula>
        <errorMessage>You must submit an Approval Request if the Number of Cards needed is above User&apos;s cards limit.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Prevent_Accepting_App_if_above_limit</fullName>
        <active>true</active>
        <description>Prevent WES Credit from making an Accepted credit decision if the requested Credit Authorised Limit or Max # of cards is above their user limit.</description>
        <errorConditionFormula>AND(
TEXT(Application_Status__c) = &quot;Accepted&quot; ||
TEXT(Credit_Decision__c) = &quot;Accepted&quot;,
BEGINS($UserRole.Name , &quot;WES Credit&quot;),
CreditLevelRequestApproved__c = FALSE,
Credit_Authorised_Limit__c &gt; $User.WES_Approval_Threshold__c ||
Total_Fuel_Cards_Needed__c &gt; $User.WES_MaxCardsCanApprove__c 
)</errorConditionFormula>
        <errorDisplayField>Credit_Decision__c</errorDisplayField>
        <errorMessage>The Credit Authorised Limit or Number of cards requested is above your limit. Please set the Credit decision to Pending Approval in order to proceed.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Prevent_Credit_from_Editing</fullName>
        <active>true</active>
        <description>Prevents Credit from Editing the Application while the application is in a status that is owned by Sales.</description>
        <errorConditionFormula>AND(
BEGINS($UserRole.Name,&quot;WES Credit&quot;),
NOT(ISCHANGED(Application_Status__c)), 
OR(				
TEXT(Application_Status__c) = &quot;Draft&quot;,			
TEXT(Application_Status__c)	= &quot;Awaiting Security Check&quot;,
TEXT(Application_Status__c)	= &quot;Sales to Obtain Further Information&quot;,	
TEXT(Application_Status__c)	= &quot;Unable to Process&quot;
))</errorConditionFormula>
        <errorMessage>Credit can&apos;t edit this Application because it is set to a status that only Sales can edit.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Prevent_Credit_from_changing_Company</fullName>
        <active>true</active>
        <description>A credit user should not be able to change the company for an application</description>
        <errorConditionFormula>AND(
BEGINS($Profile.Name , &quot;WES Credit&quot;),
ISCHANGED(Account__c)
)</errorConditionFormula>
        <errorDisplayField>Account__c</errorDisplayField>
        <errorMessage>WES Credit users can&apos;t change the company on an application.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Prevent_Editing_on_Accepted_Apps</fullName>
        <active>true</active>
        <description>Once an application is Accepted it should not be able to be edited unless user is a System Admin or WES System Admin.</description>
        <errorConditionFormula>AND(
RecordLocked__c = TRUE,
Send_App_to_IFCS__c = TRUE,
NOT(ISCHANGED(IFCS_Request_Body__c)), 	
NOT(ISBLANK(IFCS_Request_Body__c)), 
$User.ProfileId &lt;&gt; &quot;00e70000000tKvI&quot;,
$User.ProfileId &lt;&gt; &quot;00ee0000000Qed4&quot;
)</errorConditionFormula>
        <errorMessage>You can not edit an application once it has been Accepted. Seek assistance from a System admin if the application needs to be resubmitted.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Prevent_Sales_from_Editing</fullName>
        <active>true</active>
        <description>Prevents Sales from Editing the Application while the application is in a status that is owned by Credit.</description>
        <errorConditionFormula>AND(
BEGINS($UserRole.Name,&quot;WES&quot;),
CONTAINS($UserRole.Name,&quot;Sales&quot;) ||
CONTAINS($UserRole.Name,&quot;sales&quot;),
NOT(ISCHANGED(Application_Status__c)), 
OR(				
TEXT(Application_Status__c) = &quot;Awaiting Assignment&quot;,			
TEXT(Application_Status__c)	= &quot;Awaiting Credit Check&quot;,
TEXT(Application_Status__c)	= &quot;Being Credit Checked&quot;,	
TEXT(Application_Status__c)	= &quot;Awaiting Further Information&quot;,
TEXT(Application_Status__c)	= &quot;Returned from Sales&quot;,
TEXT(Application_Status__c)	= &quot;Accepted&quot;
))</errorConditionFormula>
        <errorMessage>Sales can&apos;t edit this Application because it is set to a status that only Credit can edit.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Provide_Comments_for_VP_Override</fullName>
        <active>true</active>
        <description>User must provide comments if the Velocity Profile Override is different than the calculated Velocity Profile</description>
        <errorConditionFormula>AND(
NOT(ISBLANK(TEXT(VelocityProfileOverride__c))),	
VelocityProfile__c  &lt;&gt;  TEXT(VelocityProfileOverride__c),
ISBLANK(VelocityProfileOverrideComments__c)
				)</errorConditionFormula>
        <errorDisplayField>VelocityProfileOverrideComments__c</errorDisplayField>
        <errorMessage>Please provide comments when using the Velocity Profile Override.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Provide_Reason_Security_Check_Failed</fullName>
        <active>true</active>
        <description>User must provide a reason if security check failed</description>
        <errorConditionFormula>Failed_Security_Check__c = TRUE &amp;&amp;
ISBLANK(TEXT(Security_Check_Failure_Reason__c))</errorConditionFormula>
        <errorDisplayField>Security_Check_Failure_Reason__c</errorDisplayField>
        <errorMessage>You must provide a Security Check Failure Reason</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Provide_Reason_if_Sense_Check_Fails</fullName>
        <active>true</active>
        <description>Must provide a reason if sense check is marked as Failed</description>
        <errorConditionFormula>TEXT(SenseCheck__c) = &quot;Failed&quot; &amp;&amp;
ISBLANK(Sense_Check_Failure_Reason__c)</errorConditionFormula>
        <errorDisplayField>Sense_Check_Failure_Reason__c</errorDisplayField>
        <errorMessage>Provide a reason why sense check Failed.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Ready_for_Credit_Decision</fullName>
        <active>true</active>
        <description>Information is complete enough for credit decision.</description>
        <errorConditionFormula>AND(
ISCHANGED(Credit_Decision__c),
TEXT(Credit_Decision__c) = &quot;Accepted&quot;,
From_Radius__c = FALSE,
ReadyforCreditDecision__c = FALSE
)</errorConditionFormula>
        <errorMessage>The following checks and fields must be complete before an &quot;Accepted&quot; credit decision can be made: Sense Check, Existing Account Check, Fraud Check, Periodic Assessment, Dunning Classification, Risk Category.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>WES_Application_Acceptance_Reqs</fullName>
        <active>true</active>
        <description>Here are the requirements for an application have a status of accepted.</description>
        <errorConditionFormula>AND(
TEXT(Application_Status__c) = &quot;Accepted&quot;,
From_Radius__c = FALSE, 
OR(
ISBLANK(Credit_Authorised_Limit__c),
ISBLANK(Credit_Decision_Comments__c),
ISBLANK(TEXT(OBU_Device_Required__c)),		  
TEXT(Credit_Decision__c)  &lt;&gt; &quot;Accepted&quot;
))</errorConditionFormula>
        <errorMessage>Application must have a Credit Authorised Limit and Credit Decision Comment to be marked as &quot;Accepted&quot;. It must also include an OBU Device required answer.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>WES_Cant_edit_Signatory_if_Phone_DD</fullName>
        <active>true</active>
        <description>If the DD Mandate method is phone, the primary contact will get their name stamped as the DD Signatory once the script is successfully complete.</description>
        <errorConditionFormula>TEXT(DD_Mandate_Method__c) = &quot;Phone&quot; &amp;&amp;
ISCHANGED(nameofSignature__c)</errorConditionFormula>
        <errorDisplayField>nameofSignature__c</errorDisplayField>
        <errorMessage>Can&apos;t change the Name of Signatory for a Phone DD Mandate. Must change the Primary Contact and complete the DD Mandate script successfully.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>WES_Only_assign_to_WES_queues</fullName>
        <active>true</active>
        <description>Owner can only be assigned to WES related Queues</description>
        <errorConditionFormula>NOT(BEGINS(Owner:Queue.QueueName,&quot;WES&quot;))</errorConditionFormula>
        <errorMessage>You can only select queues that belong to WES.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>WES_Pass_Security_Check_before_Credit</fullName>
        <active>true</active>
        <description>Security Check must be approved so it can be assigned to Credit.</description>
        <errorConditionFormula>AND(
TEXT(Application_Status__c) = &quot;Awaiting Assignment&quot;,
Passed_Security_Check__c = FALSE
				)</errorConditionFormula>
        <errorMessage>You must submit an Approval Request for the Security Request before it can be passed to Credit for Assignment.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>WES_Prevent_moving_back_to_Awaiting</fullName>
        <active>true</active>
        <description>Once a WES credit analyst has begun working on an Application, they can&apos;t move the application status backwards to Awaiting credit check.</description>
        <errorConditionFormula>AND(
$User.Role_Name__c = &quot;WES Credit Vetting Analyst&quot;,
TEXT(Application_Status__c) = &quot;Awaiting Credit Check&quot;,
ISCHANGED(Application_Status__c)
)</errorConditionFormula>
        <errorMessage>You can not change the Application Status to &quot;Awaiting Credit Check&quot;. You must get your manager&apos;s assistance to make this change.</errorMessage>
    </validationRules>
</CustomObject>
