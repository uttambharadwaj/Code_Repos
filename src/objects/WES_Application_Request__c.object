    <fields>
        <fullName>Risk_Category_Formula__c</fullName>
        <externalId>false</externalId>
        <formula>IF(DNB_Failure_Score__c &gt; 80, &quot;Very Low&quot;,
IF(DNB_Failure_Score__c &gt; 55 &amp;&amp; DNB_Failure_Score__c &lt;= 80, &quot;Low&quot;,
IF(DNB_Failure_Score__c &gt; 30 &amp;&amp; DNB_Failure_Score__c &lt;= 55, &quot;Medium&quot;,
IF(DNB_Failure_Score__c &gt; 25 &amp;&amp; DNB_Failure_Score__c &lt;= 30, &quot;High&quot;,
IF(DNB_Failure_Score__c &lt;= 25, &quot;Very High&quot;,
IF(Equifax_Score__c  &gt;= 505, &quot;Very Low&quot;,
IF(Equifax_Score__c  &gt;= 390 &amp;&amp; Equifax_Score__c  &lt; 505, &quot;Low&quot;,
IF(Equifax_Score__c  &gt;= 210 &amp;&amp; Equifax_Score__c  &lt; 390, &quot;Medium&quot;,
IF(Equifax_Score__c  &gt;= 120 &amp;&amp; Equifax_Score__c  &lt; 210, &quot;High&quot;,
IF(Equifax_Score__c  &lt; 120, &quot;Very High&quot;,
NULL))))))))))</formula>
