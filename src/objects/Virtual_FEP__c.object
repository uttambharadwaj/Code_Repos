    <validationRules>
        <fullName>Contract_Expiry_Or_Evergreen_Account_Req</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
  /* 
  Custom Setting - WE Validation Rules Controller - allows rule to be deactivated when the setting is changed 
  */ 
  $Setup.WE_VRC__c.All_FEP_Disabled__c = FALSE,
  /*
  Custom Setting - rule should apply to virtual US users only
  */
  $Setup.VProfileId__c.NA_Virtual_User__c = TRUE,
  /*
  Rule should not apply to NA Sales users who&apos;re creating an FEP from the Opportunity, using the New_FEP_UK button, which inserts the record before it can be edited
  */
  OR(
    AND(
      $Setup.VProfileId__c.Virtual_NA_Sales__c = TRUE,
      NOT(ISNEW())
    ),
    $Setup.VProfileId__c.Virtual_NA_Sales__c = FALSE
  ),
  Evergreen_Account__c = FALSE,
  ISBLANK(Contract_Expiry_Date__c)
)</errorConditionFormula>
        <errorMessage>Please enter the customer&apos;s Contract Expiry Date or if their contract is not due to expiry on a set date, check the Evergreen Account checkbox</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Detail_Records_Required</fullName>
        <active>false</active>
        <description>At least one Spend Detail record needs to be added by the salesperson</description>
        <errorConditionFormula>AND(

/* Allow the FEP to be created without these records(they can&apos;t be completed on the edit page)
*/
NOT(
ISNEW()),

/* Allow the FEP to be edited, while Addressable Annual Spend is not recorded
*/
NOT(
ISBLANK(Annual_Addressable_Spend__c)),

/* Allow Addressable Annual Spend to be added
*/
NOT(
AND(
ISCHANGED(Annual_Addressable_Spend__c),
OR(
ISBLANK(PRIORVALUE(Annual_Addressable_Spend__c)),
PRIORVALUE(Annual_Addressable_Spend__c) = 0
))),

/* Salesperson may not know all Spend Types but should know some
*/
Spend_Type_Of_Spend_Recorded__c  &lt; 0.01

)</errorConditionFormula>
        <errorMessage>Please add at least one Spend Type &amp; Payment Method with a share of spend that is greater than 0%</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Start_Of_Fiscal_Year_In_Past</fullName>
        <active>true</active>
        <errorConditionFormula>OR(
  AND(
    ISNEW(),
    NOT(ISBLANK(Start_Of_Fiscal_Year__c)),
    Start_Of_Fiscal_Year__c &lt; TODAY()
    ),
  AND(
    ISCHANGED(Start_Of_Fiscal_Year__c),
    NOT(ISBLANK(Start_Of_Fiscal_Year__c)),
    Start_Of_Fiscal_Year__c &lt; TODAY()
    )
)</errorConditionFormula>
        <errorDisplayField>Start_Of_Fiscal_Year__c</errorDisplayField>
        <errorMessage>Please enter the date when the customer&apos;s Next fiscal year will begin, this date cannot be in the past</errorMessage>
    </validationRules>
