<?xml version="1.0" encoding="UTF-8"?>
<CustomObject xmlns="http://soap.sforce.com/2006/04/metadata">
    <listViews>
        <fullName>All</fullName>
        <filterScope>Everything</filterScope>
        <label>All</label>
    </listViews>
    <validationRules>
        <fullName>T2_Tracker_Data_Required</fullName>
        <active>true</active>
        <errorConditionFormula>AND(

ISCHANGED(Checklist_Complete__c),
Checklist_Complete__c = True,

T2_Tracker_Data_Attached__c = False

)</errorConditionFormula>
        <errorDisplayField>T2_Tracker_Data_Attached__c</errorDisplayField>
        <errorMessage>Please confirm that T2 Data has been attached to the Handover Checklist</errorMessage>
    </validationRules>
</CustomObject>
