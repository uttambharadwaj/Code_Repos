    <validationRules>
        <fullName>PIN_Validation_4_Digit_Number</fullName>
        <active>true</active>
        <description>A PIN Number must be a 4 digit Number</description>
        <errorConditionFormula>NOT(ISBLANK(PIN__c)) &amp;&amp;
(LEN(PIN__c) &lt;&gt; 4 ||
NOT(ISNUMBER(PIN__c)))</errorConditionFormula>
        <errorDisplayField>PIN__c</errorDisplayField>
        <errorMessage>PIN Must be a 4 Digit Number</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>PIN_Validation_Ascending_Numbers</fullName>
        <active>true</active>
        <description>A PIN Number can&apos;t have 4 Numbers Ascending. Ex: 1234</description>
        <errorConditionFormula>AND(
ISNUMBER(PIN__c),
VALUE(MID(PIN__c, 1,1)) +1 = VALUE(MID(PIN__c, 2,1)),
VALUE(MID(PIN__c, 2,1)) +1 = VALUE(MID(PIN__c, 3,1)),
VALUE(MID(PIN__c, 3,1)) +1 = VALUE(MID(PIN__c, 4,1))
)</errorConditionFormula>
        <errorDisplayField>PIN__c</errorDisplayField>
        <errorMessage>PIN Can NOT be 4 numbers in Ascending order (Ex: 1234)</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>PIN_Validation_Consecutive_Numbers</fullName>
        <active>true</active>
        <description>A PIN Number can&apos;t have 4 consecutive Numbers</description>
        <errorConditionFormula>AND(
ISNUMBER(PIN__c), 
MID(PIN__c, 1,1) = MID(PIN__c, 2,1),
MID(PIN__c, 1,1) = MID(PIN__c, 3,1),
MID(PIN__c, 1,1) = MID(PIN__c, 4,1)
)</errorConditionFormula>
        <errorDisplayField>PIN__c</errorDisplayField>
        <errorMessage>PIN Can NOT be 4 consecutive numbers</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>PIN_Validation_Descending_Numbers</fullName>
        <active>true</active>
        <description>A PIN Number can&apos;t have 4 Numbers Descending. Ex: 4321</description>
        <errorConditionFormula>AND(
ISNUMBER(PIN__c),
VALUE(MID(PIN__c, 1,1)) -1 = VALUE(MID(PIN__c, 2,1)),
VALUE(MID(PIN__c, 2,1)) -1 = VALUE(MID(PIN__c, 3,1)),
VALUE(MID(PIN__c, 3,1)) -1 = VALUE(MID(PIN__c, 4,1))
)</errorConditionFormula>
        <errorDisplayField>PIN__c</errorDisplayField>
        <errorMessage>PIN Can NOT be 4 numbers in Descending order (Ex: 4321)</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Prevent_Saving_PIN_if_System_Generated</fullName>
        <active>true</active>
        <description>User can not enter and save a PIN number if they select System Generated as PIN Creation Type</description>
        <errorConditionFormula>AND(
TEXT(PIN_Creation_Method__c) = &quot;System generated&quot;,
	NOT(ISBLANK(PIN__c))
)</errorConditionFormula>
        <errorDisplayField>PIN__c</errorDisplayField>
        <errorMessage>You can not enter a PIN if you select &quot;System Generated&quot; as PIN Creation Method</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>WES_Enter_PIN_if_Fleet_User_Defined</fullName>
        <active>true</active>
        <description>Must enter a PIN if Fleet PIN and User defined</description>
        <errorConditionFormula>AND(
TEXT(PIN_Type__c) = &quot;Fleet PIN&quot;,
TEXT(PIN_Creation_Method__c) = &quot;User defined&quot;,
ISBLANK(PIN__c)
)</errorConditionFormula>
        <errorDisplayField>PIN__c</errorDisplayField>
        <errorMessage>You must enter a PIN if Fleet PIN and User defined are selected</errorMessage>
    </validationRules>
</CustomObject>
