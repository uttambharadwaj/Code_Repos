    <fields>
        <fullName>Fuel_Card_Complete_Formula__c</fullName>
        <externalId>false</externalId>
        <formula>IF(
AND(
TEXT(Card_Type__c) = &quot;EDC&quot; ||
TEXT(Card_Type__c) = &quot;UK Fuels&quot;,
NOT(ISBLANK(TEXT(POS_Prompt__c))),
NOT(ISBLANK(TEXT(Purchase_Option_Fuel__c ))), 
NOT(ISBLANK(TEXT(Velocity_Control_Profiler__c))) 
)
,TRUE,
IF(
AND(		
TEXT(Card_Type__c) = &quot;Esso&quot;,
NOT(ISBLANK(TEXT(POS_Prompt__c))),
NOT(ISBLANK(TEXT(Purchase_Option_Fuel__c ))), 
NOT(ISBLANK(TEXT(Velocity_Control_Profiler__c))), 
NOT(ISBLANK(TEXT(Toll_Usage__c))),
NOT(ISBLANK(TEXT(Purchase_Option_Services__c))),
NOT(ISBLANK(TEXT(Purchase_Option_Fuel__c))),
NOT(ISBLANK(TEXT( Purchase_Option_Products__c))),
NOT(ISBLANK(TEXT(PIN_Type__c))),
NOT(ISBLANK(TEXT(PIN_Creation_Method__c))),
NOT(ISBLANK(TEXT(National_International__c))),
NOT(ISBLANK(TEXT(Truck_Fleet__c))),
NOT(ISBLANK(TEXT(Other_Networks__c)))
)
,TRUE,FALSE))</formula>
        <label>Fuel Card Has Complete Information?</label>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
