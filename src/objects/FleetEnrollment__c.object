        <externalId>false</externalId>
        <formula>MAX ( 0, 
( Completed_Date__c - Submitted_Date__c+ CASE ( MOD (Completed_Date__c- DATE( 1900,1,6 ),7),0,1,1,1,0 ) ) - 
( ( FLOOR ( ( ( Completed_Date__c- Submitted_Date__c ) / 7 ) ) * 2 ) + 
CASE ( 
MOD ( Submitted_Date__c - DATE( 1900,1,6 ),7), 
0,CASE( MOD ( Completed_Date__c - DATE( 1900,1,6 ),7),0,1,2 ), 
1,CASE( MOD ( Completed_Date__c - DATE( 1900,1,6 ),7),0,2,1 ), 
IF ( MOD ( Submitted_Date__c - DATE( 1900,1,6 ),7) - MOD ( Completed_Date__c - DATE( 1900,1,6 ),7) &lt;= 0 ,0, 
IF ( MOD ( Completed_Date__c - DATE( 1900,1,6 ),7) = 0,1,2 ) ) ) ) )</formula>
