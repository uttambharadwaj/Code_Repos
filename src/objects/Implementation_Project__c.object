    <validationRules>
        <fullName>Max_cash_advance_percent</fullName>
        <active>true</active>
        <errorConditionFormula>100*(Maximum_Cash_Advance_Percent__c ) &gt;=  value(text(Cash_Advance__c))</errorConditionFormula>
        <errorMessage>Maximum Cash Advance Percent cannot be higher than the Cash Advance Percent Established During Discovery Stage.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Reason_Delayed_Required</fullName>
        <active>false</active>
        <errorConditionFormula>AND(
ISPICKVAL( Stage__c,&quot;Delayed&quot;),
ISBLANK(Reason_Delayed__c),
RecordTypeId=(&quot;012700000005nAH&quot;))</errorConditionFormula>
        <errorMessage>Please Fill in a Reason Delayed</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Reason_Withdrawn_Required</fullName>
        <active>false</active>
        <errorConditionFormula>AND(
ISPICKVAL( Stage__c,&quot;Withdrawn&quot;),
ISBLANK(Reason_Withdrawn__c),
RecordTypeId=(&quot;012700000005nAH&quot;))</errorConditionFormula>
        <errorDisplayField>Reason_Withdrawn__c</errorDisplayField>
        <errorMessage>Please Fill in a Reason Withdrawn</errorMessage>
    </validationRules>
</CustomObject>
