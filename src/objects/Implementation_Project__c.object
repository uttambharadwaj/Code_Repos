        <columns>FSR_Estimated_on_contract_signature__c</columns>
        <columns>Implementation_Manager__c</columns>
        <columns>Implementation_Revenue_as_a_of_FSR__c</columns>
        <columns>Implementation_Revenue_Start_Date__c</columns>
        <columns>Implementation_Product_Interest__c</columns>
        <columns>OWNER.ALIAS</columns>
        <columns>OWNER.FIRST_NAME</columns>
        <columns>OWNER.LAST_NAME</columns>
        <filterScope>Everything</filterScope>
        <filters>
            <field>RECORDTYPE</field>
            <operation>equals</operation>
            <value>Implementation_Project__c.CorporatePay_Implementation</value>
        </filters>
        <label>CorporatePay Implementations</label>
        <sharedTo>
            <group>CorporatePay2</group>
            <group>Wex_Europe</group>
        </sharedTo>
    </listViews>
    <listViews>
        <fullName>Kate_s_WEXSmart_Implementations</fullName>
        <columns>NAME</columns>
        <columns>Stage__c</columns>
        <columns>Comments__c</columns>
        <columns>Vendor__c</columns>
        <filterScope>Everything</filterScope>
        <filters>
            <field>OWNER.FIRST_NAME</field>
            <operation>equals</operation>
            <value>Kathleen</value>
        </filters>
        <label>Kate&apos;s Telematics Implementations</label>
        <sharedTo>
            <group>Telematics</group>
            <group>WEXSmart</group>
        </sharedTo>
    </listViews>
    <listViews>
        <fullName>Mallory_s_Implementations</fullName>
        <columns>NAME</columns>
        <columns>Stage__c</columns>
        <columns>Comments__c</columns>
        <columns>Vendor__c</columns>
        <filterScope>Everything</filterScope>
        <filters>
            <field>OWNER.FIRST_NAME</field>
            <operation>equals</operation>
            <value>Mallory</value>
        </filters>
