<?xml version="1.0" encoding="UTF-8"?>
<CustomObject xmlns="http://soap.sforce.com/2006/04/metadata">
    <enableEnhancedLookup>true</enableEnhancedLookup>
    <listViews>
        <fullName>All</fullName>
        <filterScope>Everything</filterScope>
        <label>All</label>
    </listViews>
    <listViews>
        <fullName>CorporatePay_Implementation_Projects</fullName>
        <columns>NAME</columns>
        <columns>Implementation_Manager__c</columns>
        <columns>Implementation_Stage__c</columns>
        <columns>Implementation_Product_Interest__c</columns>
        <columns>Account_Asset__c</columns>
        <columns>OWNER.LAST_NAME</columns>
        <columns>CREATED_DATE</columns>
        <filterScope>Everything</filterScope>
        <filters>
            <field>RECORDTYPE</field>
            <operation>equals</operation>
            <value>Implementation_Project__c.CorporatePay_Implementation</value>
        </filters>
        <label>CorporatePay Implementation Projects</label>
        <sharedTo>
            <group>CorporatePay2</group>
            <group>Wex_Europe</group>
        </sharedTo>
    </listViews>
    <listViews>
        <fullName>CorporatePay_Implementations</fullName>
