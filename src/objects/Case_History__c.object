    <fields>
        <fullName>Case__c</fullName>
        <externalId>false</externalId>
        <inlineHelpText>The case associated with the history event</inlineHelpText>
        <label>Case</label>
        <referenceTo>Case</referenceTo>
        <relationshipLabel>Case History</relationshipLabel>
        <relationshipName>Case_History</relationshipName>
        <relationshipOrder>0</relationshipOrder>
        <reparentableMasterDetail>false</reparentableMasterDetail>
        <trackTrending>false</trackTrending>
        <type>MasterDetail</type>
        <writeRequiresMasterRead>false</writeRequiresMasterRead>
    </fields>
    <fields>
        <fullName>Complete__c</fullName>
        <defaultValue>false</defaultValue>
        <externalId>false</externalId>
        <inlineHelpText>Flag to indicate if this history record has completed. Flag is set by the process builder flow.</inlineHelpText>
        <label>Complete</label>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>Event_Duration_Minutes__c</fullName>
        <externalId>false</externalId>
        <formula>(If(Complete__c, LastModifiedDate, Now()) - CreatedDate)*24*60</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <inlineHelpText>Duration to be used in reporting.</inlineHelpText>
        <label>Event Duration Minutes</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <trackTrending>false</trackTrending>
        <type>Number</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Event_Duration__c</fullName>
        <externalId>false</externalId>
        <formula>TEXT( FLOOR( (If(Complete__c, LastModifiedDate, Now()) - CreatedDate) ) ) &amp; &quot; days &quot;
&amp; TEXT( FLOOR( MOD( (If(Complete__c, LastModifiedDate, Now()) - CreatedDate) * 24, 24 ) ) ) &amp; &quot; hours &quot;
&amp; TEXT( ROUND( MOD( (If(Complete__c, LastModifiedDate, Now()) - CreatedDate) * 24 * 60, 60 ), 0 ) ) &amp; &quot; minutes&quot;</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <inlineHelpText>Formula to show readable duration in: Days, Hours and Minutes</inlineHelpText>
        <label>Event Duration</label>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Event_Type__c</fullName>
        <externalId>false</externalId>
        <inlineHelpText>Field types tracked. This is set by the process builder flow.</inlineHelpText>
        <label>Event Type</label>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Picklist</type>
        <valueSet>
            <restricted>true</restricted>
            <valueSetDefinition>
                <sorted>false</sorted>
                <value>
                    <fullName>Ownership</fullName>
                    <default>false</default>
                    <label>Ownership</label>
                </value>
                <value>
                    <fullName>Status</fullName>
                    <default>false</default>
                    <label>Status</label>
                </value>
                <value>
                    <fullName>Priority</fullName>
                    <default>false</default>
                    <label>Priority</label>
                </value>
            </valueSetDefinition>
        </valueSet>
    </fields>
    <fields>
        <fullName>Event_Value__c</fullName>
        <encryptionScheme>None</encryptionScheme>
        <externalId>false</externalId>
        <inlineHelpText>This is the field value that is changed, this will accept any value currently set up and being used. This is set by the process builder flow.</inlineHelpText>
        <label>Event Value</label>
        <length>255</length>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Ownership_Type__c</fullName>
        <externalId>false</externalId>
        <formula>IF(IsPickVal(Event_Type__c, &apos;Ownership&apos;)  &amp;&amp;   Begins(Event_Value__c, &apos;Serv Ops - &apos;), &apos;Queue&apos;, &apos;User&apos;)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Ownership Type</label>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
