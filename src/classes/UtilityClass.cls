    //custom get method for use with inner classes/wrappers, bc they don't have standard get functionality
    public static Object get(String myVariable, Object myObject){
        String jsonThis = JSON.serialize(myObject);
        Map<String, Object> mapThis = (Map<String, Object>)JSON.deserializeUntyped(jsonThis);
        return mapThis.get(myVariable);
    }

    //sometimes we need to know what an object is but we only have an id. enter this method.
    public static String getObjName(Id myId){
        String myObjectName;
        String myIdPrefix = String.valueOf(myId).substring(0,3);
        Map<String, Schema.SObjectType> mySchema = Schema.getGlobalDescribe();
        for(Schema.SObjectType sot : mySchema.values()){
            Schema.DescribeSObjectResult myResult = sot.getDescribe();
            String prefix = myResult.getKeyPrefix();
            //Check if the prefix matches with requested prefix
            if(prefix!=null && prefix.equals(myIdPrefix)){
                myObjectName = myResult.getName();
                break;
            }
        }
        return myObjectName;
    }
    //get record type id using developer name...no query!
    public static Id getRecTypeByDevName(String objName, String recTypeName){
        return Schema.getGlobalDescribe().get(objName).getDescribe().getRecordTypeInfosByDeveloperName().get(recTypeName).getRecordTypeId();
    }

    //get bunch of record type ids using developer name...no query!
    public static Set<Id> getRecTypeByDevName(String objName, List<String> recTypeNames){
        Set<Id> myIds = new Set<Id>();
        for(String s : recTypeNames)
            myIds.add(Schema.getGlobalDescribe().get(objName).getDescribe().getRecordTypeInfosByDeveloperName().get(s).getRecordTypeId());
        return myIds;
    }

    //get a record type using just object name and record type name
    public static String getRecordTypeByName(SObjectType objType, String recTypeName) {
        if(objType.getDescribe().getRecordTypeInfosByName().get(recTypeName) != null)
            return objType.getDescribe().getRecordTypeInfosByName().get(recTypeName).getRecordTypeId();
        return null;
    }
